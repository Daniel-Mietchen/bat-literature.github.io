        "data": {
            "key": "LUM3V9DV",
            "version": 35,
            "itemType": "journalArticle",
            "title": "Adaptive echolocation behavior in bats for the analysis of auditory scenes",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Chen",
                    "lastName": "Chiu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Wei",
                    "lastName": "Xian"
