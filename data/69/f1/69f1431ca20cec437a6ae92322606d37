            "key": "J6AICNQF",
            "version": 13702,
            "itemType": "journalArticle",
            "title": "Assessing the completeness of bat biodiversity inventories using species accumulation curves",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Claudia E.",
                    "lastName": "Moreno"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gonzalo",
                    "lastName": "Halffter"
                }
