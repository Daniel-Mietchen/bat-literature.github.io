            "key": "IK3CAKGX",
            "version": 11600,
            "itemType": "journalArticle",
            "title": "Gene prediction in the immunoglobulin loci",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Vikram",
                    "lastName": "Sirupurapu"
                },
                {
                    "creatorType": "author",
