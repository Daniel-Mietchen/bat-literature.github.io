            "key": "P7S9LGLA",
            "version": 17386,
            "itemType": "journalArticle",
            "title": "Contrasting thermal strategies of montane Neotropical bats at high elevations",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "I.",
                    "lastName": "Garin"
                },
                {
                    "creatorType": "author",
                    "firstName": "G.",
                    "lastName": "Chaverri"
                },
                {
                    "creatorType": "author",
                    "firstName": "L.",
                    "lastName": "Jimenez"
