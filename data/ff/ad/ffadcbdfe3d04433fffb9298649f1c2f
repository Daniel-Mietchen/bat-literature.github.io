            "version": 17397,
            "itemType": "journalArticle",
            "title": "NEW RECORD OF <i>Rhinopoma microphyllum</i> WITH THE INVESTED PARASITES AND MICROBES IN THE DEVELOPED ECOTOURISM AREA OF SOUTH LOMBOK ISLAND NTB",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "S R",
                    "lastName": "Fajri"
                },
                {
                    "creatorType": "author",
                    "firstName": "S",
                    "lastName": "NPrimawati"
                },
                {
                    "creatorType": "author",
                    "firstName": "I",
