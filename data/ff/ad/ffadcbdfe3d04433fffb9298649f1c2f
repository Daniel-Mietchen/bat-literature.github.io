            "key": "3ZDXTFTS",
            "version": 17393,
            "itemType": "journalArticle",
            "title": "Phenotypic traits evolution and morphological traits associated with echolocation calls in cryptic horseshoe bats (Rhinolophidae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ada",
                    "lastName": "Chornelia"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alice C.",
                    "lastName": "Hughes"
