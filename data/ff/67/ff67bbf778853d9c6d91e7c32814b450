            "version": 35092,
            "itemType": "journalArticle",
            "title": "Effects of Host Species Identity and Diet on the Biodiversity of the Microbiota in Puerto Rican Bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Steven J.",
                    "lastName": "Presley"
                },
                {
                    "creatorType": "author",
                    "firstName": "Joerg",
                    "lastName": "Graf"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ahmad F.",
                    "lastName": "Hassan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Anna R.",
