            "version": 28915,
            "itemType": "journalArticle",
            "title": "Plasticity in echolocation calls of Myotis macrodactylus (Chiroptera: Vespertilionidae): implications for acoustic identification",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jin-hong",
                    "lastName": "Luo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Wei",
                    "lastName": "Ou"
                },
