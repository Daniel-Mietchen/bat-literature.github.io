            "version": 28907,
            "itemType": "journalArticle",
            "title": "Bee Markers: A Novel Method for Non-Invasive Short Term Marking of Bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Lucinda",
                    "lastName": "Kirkpatrick"
                },
                {
                    "creatorType": "author",
                    "firstName": "Grzegorz",
                    "lastName": "Apoznański"
