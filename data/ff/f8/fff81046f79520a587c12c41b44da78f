            "key": "7GTW32HY",
            "version": 17932,
            "itemType": "journalArticle",
            "title": "Cave selection and use by swarming bat species",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Anita M.",
                    "lastName": "Glover"
                },
                {
                    "creatorType": "author",
                    "firstName": "John D.",
                    "lastName": "Altringham"
                }
