            "version": 55760,
            "itemType": "journalArticle",
            "title": "Wing: A suitable nonlethal tissue type for repeatable and rapid telomere length estimates in bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Megan L.",
                    "lastName": "Power"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sarahjane",
                    "lastName": "Power"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mads F.",
                    "lastName": "Bertelsen"
