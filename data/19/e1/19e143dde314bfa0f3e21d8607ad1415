            "key": "LDWJRPLD",
            "version": 29094,
            "itemType": "journalArticle",
            "title": "Bats and bell holes: The microclimatic impact of bat roosting, using a case study from Runaway Bay Caves, Jamaica",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Joyce",
                    "lastName": "Lundberg"
                },
                {
                    "creatorType": "author",
                    "firstName": "Donald A.",
                    "lastName": "McFarlane"
                }
