            "key": "65K3HSYV",
            "version": 765,
            "itemType": "journalArticle",
            "title": "Drivers of African Filovirus (Ebola and Marburg) Outbreaks",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Patrick R.",
                    "lastName": "Stephens"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mekala",
                    "lastName": "Sundaram"
                },
                {
                    "creatorType": "author",
                    "firstName": "Susana",
                    "lastName": "Ferreira"
                },
                {
                    "creatorType": "author",
                    "firstName": "Nicole",
                    "lastName": "Gottdenker"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kaniz Fatema",
                    "lastName": "Nipa"
                },
                {
                    "creatorType": "author",
                    "firstName": "Annakate M.",
                    "lastName": "Schatz"
                },
                {
                    "creatorType": "author",
                    "firstName": "John Paul",
