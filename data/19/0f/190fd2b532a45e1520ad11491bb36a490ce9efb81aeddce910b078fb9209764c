            "key": "TCNNHGTB",
            "version": 766,
            "itemType": "journalArticle",
            "title": "Temporal and spatial limitations in global surveillance for bat filoviruses and henipaviruses",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Daniel J.",
                    "lastName": "Becker"
                },
                {
                    "creatorType": "author",
                    "firstName": "Daniel E.",
                    "lastName": "Crowley"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alex D.",
