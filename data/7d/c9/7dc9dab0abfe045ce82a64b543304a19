        "data": {
            "key": "P3Q9DSR5",
            "version": 11579,
            "itemType": "journalArticle",
            "title": "Interactions between macroparasites and microparasites drive infection patterns in free-ranging African buffalo",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Anna E.",
                    "lastName": "Jolles"
                },
                {
                    "creatorType": "author",
                    "firstName": "Vanessa O.",
                    "lastName": "Ezenwa"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rampal S.",
