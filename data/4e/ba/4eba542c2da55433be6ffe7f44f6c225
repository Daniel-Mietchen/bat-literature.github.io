            "key": "63E6HDPQ",
            "version": 43938,
            "itemType": "journalArticle",
            "title": "Aging and the Environment: A Research Framework",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Andrew M.",
                    "lastName": "Geller"
                },
                {
                    "creatorType": "author",
                    "firstName": "Harold",
                    "lastName": "Zenick"
                }
