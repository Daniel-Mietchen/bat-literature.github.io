            "key": "AC2N9PW9",
            "version": 32729,
            "itemType": "journalArticle",
            "title": "Generation and characterization of bat-induced pluripotent stem cells",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Xiaohui",
                    "lastName": "Mo"
                },
