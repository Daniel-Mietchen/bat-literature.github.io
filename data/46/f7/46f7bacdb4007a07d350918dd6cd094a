            "key": "XCWW7HKI",
            "version": 34909,
            "itemType": "journalArticle",
            "title": "Phylogeny and phylogeography of Old World fruit bats in the Cynopterus brachyotis complex",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Polly",
                    "lastName": "Campbell"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christopher J.",
                    "lastName": "Schneider"
                },
                {
                    "creatorType": "author",
                    "firstName": "Adura M.",
                    "lastName": "Adnan"
                },
