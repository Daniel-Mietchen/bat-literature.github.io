            "version": 17580,
            "itemType": "journalArticle",
            "title": "Efficient Mammal Biodiversity Surveys for Ecological Restoration Monitoring",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Nicholas S",
                    "lastName": "Green"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mark L",
                    "lastName": "Wildhaber"
                },
                {
                    "creatorType": "author",
                    "firstName": "Janice L",
                    "lastName": "Albers"
