            "version": 312,
            "itemType": "journalArticle",
            "title": "Identification of key molecules in COVID-19 patients significantly correlated with clinical outcomes by analyzing transcriptomic data",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Zehua",
                    "lastName": "Dong"
                },
                {
                    "creatorType": "author",
                    "firstName": "Qiyu",
                    "lastName": "Yan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Wenxiu",
                    "lastName": "Cao"
