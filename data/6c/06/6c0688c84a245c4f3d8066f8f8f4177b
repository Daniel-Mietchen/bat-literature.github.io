            "version": 13813,
            "itemType": "journalArticle",
            "title": "Predicting Extinction Risk for Data Deficient Bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jessica Nicole",
                    "lastName": "Welch"
                },
