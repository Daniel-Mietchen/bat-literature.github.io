            "version": 44040,
            "itemType": "journalArticle",
            "title": "The Exposome in Human Evolution: From Dust to Diesel",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Benjamin C.",
                    "lastName": "Trumble"
                },
                {
                    "creatorType": "author",
                    "firstName": "Caleb E.",
                    "lastName": "Finch"
