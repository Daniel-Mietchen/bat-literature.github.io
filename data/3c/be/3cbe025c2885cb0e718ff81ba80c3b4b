                {
                    "creatorType": "author",
                    "firstName": "Caitlin M.",
                    "lastName": "Rivers"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jeffrey",
                    "lastName": "Shaman"
                },
                {
                    "creatorType": "author",
                    "firstName": "John M.",
                    "lastName": "Drake"
                },
                {
                    "creatorType": "author",
                    "firstName": "Eric",
