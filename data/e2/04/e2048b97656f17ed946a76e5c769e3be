            "version": 16789,
            "itemType": "journalArticle",
            "title": "Electroejaculation and semen buffer evaluation in the microbat Carollia perspicillata",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Nicolas Jean",
                    "lastName": "Fasel"
                },
                {
                    "creatorType": "author",
                    "firstName": "Fabrice",
                    "lastName": "Helfenstein"
                },
                {
                    "creatorType": "author",
                    "firstName": "Samuel",
                    "lastName": "Buff"
                },
                {
                    "creatorType": "author",
                    "firstName": "Heinz",
                    "lastName": "Richner"
