            "key": "WU9MXHUL",
            "version": 43703,
            "itemType": "journalArticle",
            "title": "Use of wildlife passages by invertebrate and vertebrate species",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Bertram",
                    "lastName": "Georgii"
                },
                {
                    "creatorType": "author",
                    "firstName": "Verena",
                    "lastName": "Keller"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hans Peter",
