            "version": 45733,
            "itemType": "journalArticle",
            "title": "Flight performance, echolocation and foraging behaviour in pond bats, <i>Myotis dasycneme</i> (Chiroptera: Vespertilionidae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "A. R. C.",
                    "lastName": "Britton"
                },
                {
                    "creatorType": "author",
                    "firstName": "G.",
                    "lastName": "Jones"
                },
                {
                    "creatorType": "author",
                    "firstName": "J. M. V.",
                    "lastName": "Rayner"
