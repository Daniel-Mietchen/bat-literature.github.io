            "version": 28939,
            "itemType": "journalArticle",
            "title": "Effects of Different-Syllable Aggressive Calls on Food Intake and Gene Expression in Vespertilio sinensis",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Xin",
                    "lastName": "Li"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ruizhu",
                    "lastName": "Zhou"
                },
                {
                    "creatorType": "author",
                    "firstName": "Lei",
                    "lastName": "Feng"
                },
