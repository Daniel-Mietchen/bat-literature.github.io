            "key": "S6C3ISNR",
            "version": 12319,
            "itemType": "journalArticle",
            "title": "Swarming of Bats at Different Elevations in the Carpathian Mountains",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Krzysztof",
                    "lastName": "Piksa"
                },
                {
                    "creatorType": "author",
                    "firstName": "Wiesław",
                    "lastName": "Bogdanowicz"
