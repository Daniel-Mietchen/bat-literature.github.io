            "version": 12310,
            "itemType": "journalArticle",
            "title": "Foraging Behaviour and Habitat use by the European Free-Tailed Bat <i>Tadarida teniotis</i>",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "J. Tiago",
                    "lastName": "Marques"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ana",
                    "lastName": "Rainho"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mafalda",
                    "lastName": "Carapuço"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paulo",
                    "lastName": "Oliveira"
