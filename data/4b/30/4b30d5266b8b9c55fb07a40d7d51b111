            "version": 58140,
            "itemType": "journalArticle",
            "title": "Argasid Ticks of Palearctic Bats: Distribution, Host Selection, and Zoonotic Importance",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Attila D.",
                    "lastName": "Sándor"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andrei Daniel",
                    "lastName": "Mihalca"
                },
                {
                    "creatorType": "author",
                    "firstName": "Cristian",
                    "lastName": "Domşa"
                },
                {
                    "creatorType": "author",
                    "firstName": "Áron",
