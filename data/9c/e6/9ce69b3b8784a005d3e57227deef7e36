            "version": 58585,
            "itemType": "journalArticle",
            "title": "Terrestrial locomotion of the New Zealand short-tailed bat Mystacina tuberculata and the common vampire bat Desmodus rotundus",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Daniel K.",
                    "lastName": "Riskin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Stuart",
                    "lastName": "Parsons"
                },
