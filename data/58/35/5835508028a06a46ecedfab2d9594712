            "version": 15540,
            "itemType": "journalArticle",
            "title": "Genomic characterization of seven distinct bat coronaviruses in Kenya",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Y.",
                    "lastName": "Tao"
                },
                {
                    "creatorType": "author",
                    "firstName": "K.",
                    "lastName": "Tang"
                },
                {
                    "creatorType": "author",
                    "firstName": "M.",
                    "lastName": "Shi"
                },
                {
                    "creatorType": "author",
                    "firstName": "C.",
                    "lastName": "Conrardy"
                },
