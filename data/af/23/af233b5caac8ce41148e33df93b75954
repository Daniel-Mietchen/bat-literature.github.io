            "key": "SL5VM3FL",
            "version": 11579,
            "itemType": "journalArticle",
            "title": "Ecological Contexts of Index Cases and Spillover Events of Different Ebolaviruses",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Seth D.",
                    "lastName": "Judson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Robert",
                    "lastName": "Fischer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andrew",
