            "version": 186,
            "itemType": "journalArticle",
            "title": "Dynamics of viral index infections in novel hosts",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Nardus",
                    "lastName": "Mollentze"
                },
                {
                    "creatorType": "author",
                    "firstName": "Daniel G.",
                    "lastName": "Streicker"
                },
                {
                    "creatorType": "author",
                    "firstName": "Pablo R.",
                    "lastName": "Murcia"
