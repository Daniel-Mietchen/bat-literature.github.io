            "version": 34909,
            "itemType": "journalArticle",
            "title": "Comparative Roosting Ecology of <i>Cynopterus</i> (Chiroptera: Pteropodidae) Fruit Bats in Peninsular Malaysia <sup>1</sup>",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Polly",
                    "lastName": "Campbell"
                },
                {
                    "creatorType": "author",
                    "firstName": "Noah M.",
                    "lastName": "Reid"
                },
                {
                    "creatorType": "author",
                    "firstName": "Akbar",
                    "lastName": "Zubaid"
