            "version": 45184,
            "itemType": "journalArticle",
            "title": "On the Compatibility of a Conservation Ethic with Biological Science",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Mark",
                    "lastName": "Sagoff"
                }
