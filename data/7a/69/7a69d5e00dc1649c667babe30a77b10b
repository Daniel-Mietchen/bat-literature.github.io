            "version": 56245,
            "itemType": "journalArticle",
            "title": "Increased detection of rabies virus in bats in Ceará State (Northeast Brazil) after implementation of a passive surveillance programme",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Naylê Francelino Holanda",
                    "lastName": "Duarte"
                },
                {
                    "creatorType": "author",
                    "firstName": "Carlos Henrique",
                    "lastName": "Alencar"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kellyn Kessiene De Sousa",
                    "lastName": "Cavalcante"
                },
                {
                    "creatorType": "author",
                    "firstName": "Francisco Gustavo Silveira",
                    "lastName": "Correia"
                },
                {
                    "creatorType": "author",
                    "firstName": "Phyllis Catharina",
                    "lastName": "Romijn"
                },
                {
                    "creatorType": "author",
                    "firstName": "Danielle Bastos",
                    "lastName": "Araujo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Silvana Regina",
