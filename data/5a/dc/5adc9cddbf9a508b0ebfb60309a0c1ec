            "key": "656ZP4S3",
            "version": 34948,
            "itemType": "journalArticle",
            "title": "Cranial Shape and Diet Variation in <i>Myotis</i> Species (Chiroptera: Vespertilionidae): Testing the Relationship between Form and Function",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Sandra Milena",
                    "lastName": "Ospina-Garcés"
                },
                {
                    "creatorType": "author",
                    "firstName": "Efraín De",
                    "lastName": "Luna"
                },
                {
                    "creatorType": "author",
                    "firstName": "L.",
                    "lastName": "Gerardo Herrera M."
                },
