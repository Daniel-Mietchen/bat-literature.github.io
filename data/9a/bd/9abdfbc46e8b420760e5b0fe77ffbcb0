            "version": 57979,
            "itemType": "journalArticle",
            "title": "Bat echolocation in continental China: a systematic review and first acoustic identification key for the country",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "David",
                    "lastName": "López-Bosch"
                },
                {
                    "creatorType": "author",
                    "firstName": "Joe Chun-Chia",
                    "lastName": "Huang"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yanping",
                    "lastName": "Wang"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ana Filipa",
                    "lastName": "Palmeirim"
                },
                {
                    "creatorType": "author",
                    "firstName": "Luke",
