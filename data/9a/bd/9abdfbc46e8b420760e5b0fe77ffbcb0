            "version": 57984,
            "itemType": "journalArticle",
            "title": "Optimizing bat bioacoustic surveys in human‐modified Neotropical landscapes",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Adrià",
                    "lastName": "López‐Baucells"
                },
                {
                    "creatorType": "author",
                    "firstName": "Natalie",
                    "lastName": "Yoh"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ricardo",
                    "lastName": "Rocha"
