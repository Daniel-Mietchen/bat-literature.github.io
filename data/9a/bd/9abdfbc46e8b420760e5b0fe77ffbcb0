            "version": 57968,
            "itemType": "journalArticle",
            "title": "Long‐term surveillance of bat coronaviruses in Korea: Diversity and distribution pattern",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Van Thi",
                    "lastName": "Lo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sun‐Woo",
                    "lastName": "Yoon"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ji Yeong",
                    "lastName": "Noh"
                },
                {
                    "creatorType": "author",
                    "firstName": "Youngji",
                    "lastName": "Kim"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yong Gun",
