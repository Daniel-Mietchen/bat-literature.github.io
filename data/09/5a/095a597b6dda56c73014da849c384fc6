                {
                    "creatorType": "author",
                    "firstName": "Julia",
                    "lastName": "Hanmer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ida Bærholm",
                    "lastName": "Schnell"
                },
                {
                    "creatorType": "author",
                    "firstName": "Morten",
                    "lastName": "Rasmussen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Thomas P.",
