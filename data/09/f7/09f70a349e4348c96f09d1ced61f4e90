            "url": "https://dx.plos.org/10.1371/journal.pone.0146274",
            "accessDate": "2024-07-17T14:32:13Z",
            "archive": "",
            "archiveLocation": "",
            "libraryCatalog": "DOI.org (Crossref)",
            "callNumber": "",
            "rights": "",
            "extra": "",
