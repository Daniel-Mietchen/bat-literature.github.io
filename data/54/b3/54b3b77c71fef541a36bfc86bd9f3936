            "itemType": "journalArticle",
            "title": "Nectarivory by Endemic Malagasy Fruit Bats During the Dry Season",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Daudet",
                    "lastName": "Andriafidison"
                },
                {
                    "creatorType": "author",
                    "firstName": "Radosoa A.",
                    "lastName": "Andrianaivoarivelo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Olga R.",
                    "lastName": "Ramilijaona"
                },
                {
                    "creatorType": "author",
                    "firstName": "Marlène R.",
                    "lastName": "Razanahoera"
                },
                {
                    "creatorType": "author",
                    "firstName": "James",
                    "lastName": "MacKinnon"
                },
