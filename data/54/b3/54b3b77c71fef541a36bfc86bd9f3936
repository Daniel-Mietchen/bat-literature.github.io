            "key": "VIAD726V",
            "version": 54681,
            "itemType": "journalArticle",
            "title": "Temporal dynamics of European bat lyssavirus type 1 and survival of Myotis myotis bats in natural colonies",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Blanca",
                    "lastName": "Amengual"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hervé",
                    "lastName": "Bourhy"
                },
                {
                    "creatorType": "author",
                    "firstName": "Marc",
