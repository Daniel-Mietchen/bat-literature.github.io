        "data": {
            "key": "EW6RKX7A",
            "version": 11571,
            "itemType": "journalArticle",
            "title": "A Predictive Spatial Distribution Framework for Filovirus-Infected Bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "G.",
                    "lastName": "Fiorillo"
                },
                {
                    "creatorType": "author",
                    "firstName": "P.",
