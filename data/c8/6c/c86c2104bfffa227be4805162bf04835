            "itemType": "journalArticle",
            "title": "Influencing Activity of Bats by Dimly Lighting Wind Turbine Surfaces with Ultraviolet Light",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Paul M.",
                    "lastName": "Cryan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paulo M.",
                    "lastName": "Gorresen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Bethany R.",
                    "lastName": "Straw"
                },
                {
                    "creatorType": "author",
                    "firstName": "Syhoune (Simon)",
                    "lastName": "Thao"
