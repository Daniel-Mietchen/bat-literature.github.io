            "version": 42783,
            "itemType": "journalArticle",
            "title": "SYSTEMATIC STATUS OF AFRICAN POPULATIONS OF LONG-EARED BATS, GENUS PLECOTUS (MAMMALIA: CHIROPTERA)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Petr",
                    "lastName": "Benda"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andreas",
                    "lastName": "Kiefer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Vladimír",
