            "key": "6ADKBGW8",
            "version": 55959,
            "itemType": "journalArticle",
            "title": "Bat guano as new and attractive chitin and chitosan source",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Murat",
                    "lastName": "Kaya"
                },
                {
                    "creatorType": "author",
                    "firstName": "Osman",
                    "lastName": "Seyyar"
                },
                {
                    "creatorType": "author",
                    "firstName": "Talat",
