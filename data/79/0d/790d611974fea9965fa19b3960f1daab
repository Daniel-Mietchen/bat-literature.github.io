            "version": 12273,
            "itemType": "journalArticle",
            "title": "Bat Incidents with U.S. Civil Aircraft",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Kristin M.",
                    "lastName": "Biondi"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jerrold L.",
