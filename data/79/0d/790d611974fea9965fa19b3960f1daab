            "version": 12273,
            "itemType": "journalArticle",
            "title": "Population Genetics and Bat Rabies: A Case Study of <i>Eptesicus serotinus</i> in Poland",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Wiesław",
                    "lastName": "Bogdanowicz"
                },
                {
                    "creatorType": "author",
                    "firstName": "Grzegorz",
                    "lastName": "Lesiński"
                },
                {
                    "creatorType": "author",
                    "firstName": "Małgorzata",
