            "version": 57808,
            "itemType": "journalArticle",
            "title": "Drivers of bat roles in Neotropical seed dispersal networks: abundance is more important than functional traits",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Rafael De Souza",
                    "lastName": "Laurindo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jeferson",
                    "lastName": "Vizentin-Bugoni"
                },
                {
                    "creatorType": "author",
                    "firstName": "Davi Castro",
                    "lastName": "Tavares"
                },
                {
                    "creatorType": "author",
                    "firstName": "Matheus Camargo Silva",
                    "lastName": "Mancini"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rodrigo De Macêdo",
