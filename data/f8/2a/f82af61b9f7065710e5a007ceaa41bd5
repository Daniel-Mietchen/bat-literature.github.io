            "version": 18194,
            "itemType": "journalArticle",
            "title": "Ethogram of Ghost Bat (Macroderma gigas) Behaviours and Associated Social Vocalisations",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Nicola",
                    "lastName": "Hanrahan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Anastasia H.",
                    "lastName": "Dalziell"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christopher",
