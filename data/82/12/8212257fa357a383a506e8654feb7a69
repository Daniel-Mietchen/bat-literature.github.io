            "version": 29028,
            "itemType": "journalArticle",
            "title": "Adaptation of the FK506 binding protein 1B to hibernation in bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Di",
                    "lastName": "Liu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Shenghui",
                    "lastName": "Zheng"
                },
                {
                    "creatorType": "author",
                    "firstName": "Guantao",
                    "lastName": "Zheng"
