            "version": 57038,
            "itemType": "journalArticle",
            "title": "Fruit bats adjust their foraging strategies to urban environments to diversify their diet",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Katya",
                    "lastName": "Egert-Berg"
                },
                {
                    "creatorType": "author",
                    "firstName": "Michal",
                    "lastName": "Handel"
                },
                {
                    "creatorType": "author",
                    "firstName": "Aya",
                    "lastName": "Goldshtein"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ofri",
                    "lastName": "Eitan"
