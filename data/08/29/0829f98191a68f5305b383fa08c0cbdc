            "key": "3X5QYVWM",
            "version": 35238,
            "itemType": "journalArticle",
            "title": "Multimodal cues improve prey localization under complex environmental conditions",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "F.",
                    "lastName": "Rhebergen"
                },
                {
                    "creatorType": "author",
                    "firstName": "R. C.",
                    "lastName": "Taylor"
                },
                {
                    "creatorType": "author",
                    "firstName": "M. J.",
                    "lastName": "Ryan"
                },
                {
                    "creatorType": "author",
                    "firstName": "R. A.",
                    "lastName": "Page"
