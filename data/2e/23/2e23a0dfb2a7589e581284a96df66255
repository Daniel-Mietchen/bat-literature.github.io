            "key": "86TNX3JN",
            "version": 28902,
            "itemType": "journalArticle",
            "title": "Activity of Insectivorous Bats is Related to Water Availability in a Highly Modified Mexican Temperate Forest",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Celia",
                    "lastName": "López-González"
                },
                {
                    "creatorType": "author",
                    "firstName": "Abraham",
                    "lastName": "Lozano"
                },
                {
                    "creatorType": "author",
                    "firstName": "Emma P.",
                    "lastName": "Gómez-Ruiz"
                },
