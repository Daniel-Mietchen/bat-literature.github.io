            "key": "68EFPRHI",
            "version": 11579,
            "itemType": "journalArticle",
            "title": "Biomarker of burden: Feather corticosterone reflects energetic expenditure and allostatic overload in captive waterfowl",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "David W.",
                    "lastName": "Johns"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tracy A.",
                    "lastName": "Marchant"
                },
                {
                    "creatorType": "author",
                    "firstName": "Graham D.",
                    "lastName": "Fairhurst"
                },
                {
                    "creatorType": "author",
                    "firstName": "John R.",
                    "lastName": "Speakman"
