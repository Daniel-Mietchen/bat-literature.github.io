            "key": "NWMTLBZ7",
            "version": 11606,
            "itemType": "journalArticle",
            "title": "Bushmeat hunting in the Congo Basin: an assessment of impacts and options for mitigation",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "David S.",
                    "lastName": "Wilkie"
                },
                {
                    "creatorType": "author",
                    "firstName": "Julia F.",
                    "lastName": "Carpenter"
                }
