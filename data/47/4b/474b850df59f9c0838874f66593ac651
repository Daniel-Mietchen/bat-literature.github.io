            "version": 17645,
            "itemType": "journalArticle",
            "title": "The Potential to Encode Detailed Information About Parasites in the Acoustic Signals of Chinese Horseshoe Bats (Rhinolophus sinicus)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Baozhu",
                    "lastName": "Fan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yujuan",
                    "lastName": "Wang"
                },
                {
                    "creatorType": "author",
                    "firstName": "Xiaobin",
                    "lastName": "Huang"
