            "version": 35234,
            "itemType": "journalArticle",
            "title": "Specialization and omnivory in diverse mammalian assemblages",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Katja",
                    "lastName": "Rex"
                },
                {
                    "creatorType": "author",
                    "firstName": "Benjamin I.",
                    "lastName": "Czaczkes"
