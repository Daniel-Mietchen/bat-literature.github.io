            "version": 35232,
            "itemType": "journalArticle",
            "title": "Coastal onshore wind turbines lead to habitat loss for bats in Northern Germany",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Christine",
                    "lastName": "Reusch"
                },
                {
                    "creatorType": "author",
                    "firstName": "Maja",
                    "lastName": "Lozar"
                },
