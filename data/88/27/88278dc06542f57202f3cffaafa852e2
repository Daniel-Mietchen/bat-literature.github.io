            "key": "NJSN6DGJ",
            "version": 35047,
            "itemType": "journalArticle",
            "title": "Maximum entropy modeling of species geographic distributions",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Steven J.",
                    "lastName": "Phillips"
                },
                {
                    "creatorType": "author",
                    "firstName": "Robert P.",
