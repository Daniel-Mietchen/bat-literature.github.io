            "version": 55542,
            "itemType": "journalArticle",
            "title": "Borrelia in neotropical bats: Detection of two new phylogenetic lineages",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Pablo",
                    "lastName": "Colunga-Salas"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sokani",
                    "lastName": "Sánchez-Montes"
                },
                {
                    "creatorType": "author",
                    "firstName": "Livia",
                    "lastName": "León-Paniagua"
                },
