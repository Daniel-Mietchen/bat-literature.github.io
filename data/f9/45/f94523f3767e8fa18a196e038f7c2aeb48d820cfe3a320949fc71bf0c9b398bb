            "version": 116,
            "itemType": "journalArticle",
            "title": "Cold Exposure After Exercise Impedes the Neuroprotective Effects of Exercise on Thermoregulation and UCP4 Expression in an MPTP-Induced Parkinsonian Mouse Model",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Yi-Ju",
                    "lastName": "Tsai"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yue-Cih",
                    "lastName": "Jhong"
                },
                {
                    "creatorType": "author",
                    "firstName": "Shih-Hong",
                    "lastName": "Ching"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yu-Ching",
                    "lastName": "Liao"
                },
                {
                    "creatorType": "author",
                    "firstName": "Cheng-Hsin",
                    "lastName": "Ching"
