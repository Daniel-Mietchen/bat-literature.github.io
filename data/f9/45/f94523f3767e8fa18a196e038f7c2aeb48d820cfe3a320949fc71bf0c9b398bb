            "version": 110,
            "itemType": "journalArticle",
            "title": "Chronic social stress, social status, and susceptibility to upper respiratory infections in nonhuman primates",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "S.",
                    "lastName": "Cohen"
                },
                {
                    "creatorType": "author",
                    "firstName": "S.",
                    "lastName": "Line"
                },
                {
                    "creatorType": "author",
                    "firstName": "S. B.",
                    "lastName": "Manuck"
                },
                {
                    "creatorType": "author",
                    "firstName": "B. S.",
