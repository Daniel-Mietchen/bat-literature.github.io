            "volume": "108",
            "issue": "25",
            "pages": "10208-10213",
            "date": "2011/06/21",
            "series": "",
            "seriesTitle": "",
            "seriesText": "",
            "journalAbbreviation": "PNAS",
            "language": "en",
            "DOI": "10.1073/pnas.1010875108",
            "ISSN": "0027-8424, 1091-6490",
            "shortTitle": "",
            "url": "https://www.pnas.org/content/108/25/10208",
