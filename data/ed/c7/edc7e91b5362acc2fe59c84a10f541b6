            "key": "3P96ETNE",
            "version": 57282,
            "itemType": "journalArticle",
            "title": "Bats as potential suppressors of multiple agricultural pests: A case study from Madagascar",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "James",
                    "lastName": "Kemp"
                },
                {
                    "creatorType": "author",
                    "firstName": "Adri√†",
                    "lastName": "L√≥pez-Baucells"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ricardo",
                    "lastName": "Rocha"
                },
                {
                    "creatorType": "author",
                    "firstName": "Owen S.",
                    "lastName": "Wangensteen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Zo",
                    "lastName": "Andriatafika"
                },
                {
                    "creatorType": "author",
                    "firstName": "Abhilash",
