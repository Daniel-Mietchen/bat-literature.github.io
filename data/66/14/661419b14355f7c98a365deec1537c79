            "itemType": "journalArticle",
            "title": "The role of complex vegetation structures in determining hawking bat activity in temperate forests",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Joseph",
                    "lastName": "Langridge"
                },
                {
                    "creatorType": "author",
                    "firstName": "Benoît",
                    "lastName": "Pisanu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sébastien",
                    "lastName": "Laguet"
                },
                {
                    "creatorType": "author",
                    "firstName": "Frédéric",
