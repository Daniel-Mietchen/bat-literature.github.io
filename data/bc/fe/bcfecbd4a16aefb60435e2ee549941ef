            "key": "VAPIL4M3",
            "version": 56301,
            "itemType": "journalArticle",
            "title": "Bats host the most virulent—but not the most dangerous—zoonotic viruses",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Sarah",
                    "lastName": "Guth"
                },
                {
                    "creatorType": "author",
                    "firstName": "Nardus",
                    "lastName": "Mollentze"
                },
                {
                    "creatorType": "author",
                    "firstName": "Katia",
                    "lastName": "Renault"
                },
                {
                    "creatorType": "author",
                    "firstName": "Daniel G.",
                    "lastName": "Streicker"
                },
                {
                    "creatorType": "author",
                    "firstName": "Elisa",
                    "lastName": "Visher"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mike",
                    "lastName": "Boots"
