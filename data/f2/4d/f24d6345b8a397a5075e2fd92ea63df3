            "key": "7FGX99AB",
            "version": 18218,
            "itemType": "journalArticle",
            "title": "Modelling multi-scale spatial variation in species richness from abundance data in a complex neotropical bat assemblage",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Mickaël",
                    "lastName": "Henry"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jean François",
                    "lastName": "Cosson"
                },
