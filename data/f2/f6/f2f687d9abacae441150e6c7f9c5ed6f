            "version": 7681,
            "itemType": "bookSection",
            "title": "Does energy or calcium availability constrain reproduction by bats?",
            "creators": [
                {
                    "creatorType": "editor",
                    "firstName": "Paul A",
                    "lastName": "Racey"
                },
                {
                    "creatorType": "editor",
                    "firstName": "Susan M",
                    "lastName": "Swift"
