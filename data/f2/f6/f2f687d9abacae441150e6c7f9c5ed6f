            "version": 7672,
            "itemType": "journalArticle",
            "title": "The abundance and development of cavities in Eucalyptustrees: a case study in the montane forests of Victoria, southeastern Australia",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "D B",
                    "lastName": "Lindenmayer"
                },
                {
                    "creatorType": "author",
                    "firstName": "R. B.",
                    "lastName": "Cunningham"
                },
                {
                    "creatorType": "author",
                    "firstName": "C. F.",
                    "lastName": "Donnelly"
                },
                {
                    "creatorType": "author",
                    "firstName": "M.",
