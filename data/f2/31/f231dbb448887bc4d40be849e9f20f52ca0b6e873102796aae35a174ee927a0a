            "version": 262,
            "itemType": "journalArticle",
            "title": "Genetic diversity in cytokines associated with immune variation and resistance to multiple pathogens in a natural rodent population",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Andrew K.",
                    "lastName": "Turner"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mike",
                    "lastName": "Begon"
                },
                {
                    "creatorType": "author",
                    "firstName": "Joseph A.",
                    "lastName": "Jackson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Janette E.",
