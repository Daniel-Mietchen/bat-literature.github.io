            "key": "AMCFMSE8",
            "version": 276,
            "itemType": "journalArticle",
            "title": "GOrilla: a tool for discovery and visualization of enriched GO terms in ranked gene lists.",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Eran",
                    "lastName": "Eden"
                },
                {
                    "creatorType": "author",
                    "firstName": "Roy",
                    "lastName": "Navon"
                },
                {
                    "creatorType": "author",
                    "firstName": "Israel",
                    "lastName": "Steinfeld"
                },
