            "version": 17914,
            "itemType": "journalArticle",
            "title": "Social communication in bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Gloriana",
                    "lastName": "Chaverri"
                },
                {
                    "creatorType": "author",
                    "firstName": "Leonardo",
