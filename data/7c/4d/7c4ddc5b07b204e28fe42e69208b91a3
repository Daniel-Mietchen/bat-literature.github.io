            "key": "4VC3CYDA",
            "version": 17967,
            "itemType": "journalArticle",
            "title": "Does bat response to traffic noise support the misleading cue hypothesis?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Inga",
                    "lastName": "Geipel"
                },
                {
                    "creatorType": "author",
                    "firstName": "Bawan",
                    "lastName": "Amin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rachel A",
                    "lastName": "Page"
                },
                {
                    "creatorType": "author",
                    "firstName": "Wouter",
                    "lastName": "Halfwerk"
                },
