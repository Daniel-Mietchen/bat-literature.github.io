            "key": "FJWSGFMQ",
            "version": 435,
            "itemType": "journalArticle",
            "title": "Photoperiodic regulation of behavior: Peromyscus as a model system",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jeremy C.",
                    "lastName": "Borniger"
                },
                {
                    "creatorType": "author",
                    "firstName": "Randy J.",
                    "lastName": "Nelson"
