            "key": "KSQI7XI8",
            "version": 29069,
            "itemType": "journalArticle",
            "title": "Implications of in vitro bioaccessibility differences for the assessment of risks of metals to bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Béatrice V.",
                    "lastName": "Hernout"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sarah R.",
                    "lastName": "Bowman"
                },
                {
                    "creatorType": "author",
                    "firstName": "Robert J.",
                    "lastName": "Weaver"
                },
                {
                    "creatorType": "author",
                    "firstName": "Channaka J.",
                    "lastName": "Jayasinghe"
