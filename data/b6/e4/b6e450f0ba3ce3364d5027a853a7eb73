            "version": 11936,
            "itemType": "journalArticle",
            "title": "Disassembly of Fragmented Bat Communities in Orange Walk District, Belize",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "James P.",
                    "lastName": "Herrera"
                },
                {
                    "creatorType": "author",
                    "firstName": "Neil",
                    "lastName": "Duncan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Elizabeth",
                    "lastName": "Clare"
