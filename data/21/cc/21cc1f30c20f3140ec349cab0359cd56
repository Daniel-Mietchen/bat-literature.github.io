            "key": "GXDHSYPE",
            "version": 32520,
            "itemType": "journalArticle",
            "title": "Determining When to Change Course in Management Actions",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Chooi Fei",
                    "lastName": "Ng"
                },
                {
                    "creatorType": "author",
                    "firstName": "Michael A.",
                    "lastName": "Mccarthy"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tara G.",
                    "lastName": "Martin"
