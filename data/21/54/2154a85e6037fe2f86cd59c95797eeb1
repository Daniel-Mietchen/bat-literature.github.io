            "version": 13793,
            "itemType": "journalArticle",
            "title": "Indicators for taxonomic and functional aspects of biodiversity in the vineyard agroecosystem of Southern Switzerland",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "V.",
                    "lastName": "Trivellone"
                },
                {
                    "creatorType": "author",
                    "firstName": "N.",
                    "lastName": "Schoenenberger"
                },
                {
                    "creatorType": "author",
                    "firstName": "B.",
