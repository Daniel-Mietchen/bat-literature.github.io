            "key": "I84TTT8M",
            "version": 13800,
            "itemType": "journalArticle",
            "title": "Ecological Specialization and Susceptibility to Disturbance: Conjectures and Refutations",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Diego P.",
                    "lastName": "Vázquez"
                },
                {
                    "creatorType": "author",
                    "firstName": "Daniel",
                    "lastName": "Simberloff"
                }
