            "version": 54358,
            "itemType": "journalArticle",
            "title": "Cryptic Diversity in the Genus Miniopterus with the Description of a New Species from Southern Africa",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ara",
                    "lastName": "Monadjem"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jen",
                    "lastName": "Guyton"
                },
                {
                    "creatorType": "author",
                    "firstName": "Piotr",
                    "lastName": "Naskrecki"
                },
                {
                    "creatorType": "author",
                    "firstName": "Leigh R.",
                    "lastName": "Richards"
                },
                {
                    "creatorType": "author",
                    "firstName": "Anna S.",
                    "lastName": "Kropff"
                },
