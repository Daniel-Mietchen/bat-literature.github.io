        "data": {
            "key": "HGUXIADH",
            "version": 829,
            "itemType": "journalArticle",
            "title": "Antigenic characterisation of lyssaviruses in South Africa",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ernest",
                    "lastName": "Ngoepe"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christine",
                    "lastName": "Fehlner-Gardiner"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alex",
