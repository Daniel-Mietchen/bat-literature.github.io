        "data": {
            "key": "H9MN3JLT",
            "version": 39761,
            "itemType": "journalArticle",
            "title": "Characterization of pituitary–adrenocortical activity in the Malayan flying fox (Pteropus vampyrus)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "DeeAnn M.",
                    "lastName": "Reeder"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hershel",
                    "lastName": "Raff"
                },
                {
                    "creatorType": "author",
                    "firstName": "Thomas H.",
                    "lastName": "Kunz"
                },
