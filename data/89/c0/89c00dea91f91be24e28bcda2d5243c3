            "version": 39734,
            "itemType": "bookSection",
            "title": "Bats in the Anthropocene",
            "creators": [
                {
                    "creatorType": "editor",
                    "firstName": "Christian C.",
                    "lastName": "Voigt"
                },
                {
                    "creatorType": "editor",
                    "firstName": "Tigga",
