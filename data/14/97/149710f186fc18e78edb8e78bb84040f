            "key": "36RNLPX2",
            "version": 42446,
            "itemType": "journalArticle",
            "title": "Multiple morphological characters needed for field identification of cryptic long‐eared bat species around the Swiss Alps",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "S.",
                    "lastName": "Ashrafi"
                },
                {
                    "creatorType": "author",
                    "firstName": "F.",
                    "lastName": "Bontadina"
                },
                {
                    "creatorType": "author",
                    "firstName": "A.",
                    "lastName": "Kiefer"
                },
