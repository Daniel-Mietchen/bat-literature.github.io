            "title": "Hybridization Hotspots at Bat Swarming Sites",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Wiesław",
                    "lastName": "Bogdanowicz"
                },
                {
                    "creatorType": "author",
                    "firstName": "Krzysztof",
                    "lastName": "Piksa"
                },
                {
                    "creatorType": "author",
