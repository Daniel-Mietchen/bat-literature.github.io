            "key": "A2B7A9AB",
            "version": 12327,
            "itemType": "journalArticle",
            "title": "Estimating Sample Size for Landscape-Scale Mark-Recapture Studies of North American Migratory Tree Bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Robert A.",
                    "lastName": "Schorr"
                },
                {
                    "creatorType": "author",
                    "firstName": "Laura E.",
                    "lastName": "Ellison"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paul M.",
                    "lastName": "Lukacs"
                }
