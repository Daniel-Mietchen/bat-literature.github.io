            "version": 13671,
            "itemType": "journalArticle",
            "title": "Evolutionary history of mammalian sucking lice (Phthiraptera: Anoplura)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jessica E",
                    "lastName": "Light"
                },
                {
                    "creatorType": "author",
                    "firstName": "Vincent S",
                    "lastName": "Smith"
                },
                {
                    "creatorType": "author",
                    "firstName": "Julie M",
