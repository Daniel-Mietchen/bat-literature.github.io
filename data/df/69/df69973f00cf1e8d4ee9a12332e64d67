            "version": 45209,
            "itemType": "journalArticle",
            "title": "MURCIÉLAGOS DISPERSORES DE SEMILLAS EN GRADIENTES SUCESIONALES DE LA ORINOQUIA (SAN MARTÍN, META, COLOMBIA)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Diego",
                    "lastName": "Casallas-Pabón"
                },
                {
                    "creatorType": "author",
                    "firstName": "Nathaly",
                    "lastName": "Calvo-Roa"
