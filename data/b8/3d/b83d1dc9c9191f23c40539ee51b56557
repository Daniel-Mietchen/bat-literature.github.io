            "key": "MMLSHBFG",
            "version": 32484,
            "itemType": "journalArticle",
            "title": "Structural, temporal and genetic properties of social groups in the short-lived migratory bat Nyctalus leisleri",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ladislav",
                    "lastName": "Naďo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Renáta",
                    "lastName": "Chromá"
                },
