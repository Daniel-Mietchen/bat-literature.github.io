            "key": "9KSWSSHZ",
            "version": 17658,
            "itemType": "journalArticle",
            "title": "Effects of treefall gaps created by windthrow on bat assemblages in a temperate forest",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Dai",
                    "lastName": "Fukui"
                },
                {
                    "creatorType": "author",
                    "firstName": "Toshihide",
                    "lastName": "Hirao"
                },
                {
                    "creatorType": "author",
                    "firstName": "Masashi",
                    "lastName": "Murakami"
