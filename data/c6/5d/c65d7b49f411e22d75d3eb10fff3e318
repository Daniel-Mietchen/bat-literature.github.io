            "itemType": "journalArticle",
            "title": "DAY ROOSTS OF FEMALE FRINGED MYOTIS (MYOTIS THYSANODES) IN XERIC FORESTS OF THE PACIFIC NORTHWEST",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Michael J.",
                    "lastName": "Lacki"
                },
                {
                    "creatorType": "author",
                    "firstName": "Michael D.",
                    "lastName": "Baker"
                }
