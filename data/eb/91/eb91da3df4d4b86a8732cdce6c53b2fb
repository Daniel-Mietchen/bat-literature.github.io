            "itemType": "journalArticle",
            "title": "Mitochondrial phylogeography of the long-eared bats (Plecotus) in the Mediterranean Palaearctic and Atlantic Islands",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "J",
                    "lastName": "Juste"
                },
                {
                    "creatorType": "author",
                    "firstName": "C",
                    "lastName": "Ibáñez"
                },
                {
                    "creatorType": "author",
                    "firstName": "J",
                    "lastName": "Muñoz"
                },
