            "version": 58562,
            "itemType": "journalArticle",
            "title": "Identifying priority areas for bat conservation in the Western Ghats mountain range, peninsular India",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Sreehari",
                    "lastName": "Raman"
                },
                {
                    "creatorType": "author",
                    "firstName": "Thekke Thumbath",
                    "lastName": "Shameer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ushakumari",
