            "version": 29117,
            "itemType": "journalArticle",
            "title": "Moving in three dimensions: effects of structural complexity on occurrence and activity of insectivorous bats in managed forest stands",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Kirsten",
                    "lastName": "Jung"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sonja",
                    "lastName": "Kaiser"
                },
                {
                    "creatorType": "author",
                    "firstName": "Stefan",
                    "lastName": "Böhm"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jens",
                    "lastName": "Nieschulze"
