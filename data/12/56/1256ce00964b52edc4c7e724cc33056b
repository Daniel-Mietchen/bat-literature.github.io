        "data": {
            "key": "2QPHZ4AG",
            "version": 54942,
            "itemType": "journalArticle",
            "title": "Active season body mass patterns of little brown and northern myotis bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Evan W.",
                    "lastName": "Balzer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Adam D.",
                    "lastName": "Grottoli"
                },
                {
                    "creatorType": "author",
                    "firstName": "Lynne E.",
                    "lastName": "Burns"
                },
