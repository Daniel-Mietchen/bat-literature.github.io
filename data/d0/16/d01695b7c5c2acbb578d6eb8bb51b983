                {
                    "creatorType": "author",
                    "firstName": "Catherine",
                    "lastName": "Clary"
                },
                {
                    "creatorType": "author",
                    "firstName": "Victorine T.",
                    "lastName": "Maptue"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jason D.",
