            "key": "BSM7NY6R",
            "version": 29442,
            "itemType": "journalArticle",
            "title": "Wind turbine sensor array for monitoring avian and bat collisions",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Congcong",
                    "lastName": "Hu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Roberto",
                    "lastName": "Albertani"
                },
