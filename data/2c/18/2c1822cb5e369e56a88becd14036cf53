            "version": 17047,
            "itemType": "journalArticle",
            "title": "Noseleaf Dynamics during Pulse Emission in Horseshoe Bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Lin",
                    "lastName": "Feng"
                },
                {
                    "creatorType": "author",
                    "firstName": "Li",
                    "lastName": "Gao"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hongwang",
                    "lastName": "Lu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rolf",
                    "lastName": "Müller"
                },
                {
                    "creatorType": "editor",
                    "firstName": "Manuel S.",
                    "lastName": "Malmierca"
                }
