            "key": "HSLJULAD",
            "version": 2574,
            "itemType": "journalArticle",
            "title": "Coronavirus sampling and surveillance in bats from 1996-2019: a systematic review and meta-analysis",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Lily E.",
                    "lastName": "Cohen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Anna C.",
                    "lastName": "Fagre"
                },
                {
                    "creatorType": "author",
                    "firstName": "Binqi",
                    "lastName": "Chen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Colin J.",
