            "key": "ZMY7X8KV",
            "version": 46537,
            "itemType": "journalArticle",
            "title": "Mating System of the Tent-Making Bat Artibeus watsoni (Chiroptera: Phyllostomidae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Gloriana",
                    "lastName": "Chaverri"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christopher J.",
                    "lastName": "Schneider"
                },
