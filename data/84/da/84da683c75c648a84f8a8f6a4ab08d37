            "version": 38432,
            "itemType": "journalArticle",
            "title": "Phylogeny, Molecules Versus Morphology, and Rates of Character Evolution Among Fruitbats (Chiroptera: Megachiroptera)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ms",
                    "lastName": "Springer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Lj",
