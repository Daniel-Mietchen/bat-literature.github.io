                },
                {
                    "creatorType": "author",
                    "firstName": "Juan Luis",
                    "lastName": "García-Mudarra"
                },
                {
                    "creatorType": "author",
                    "firstName": "Luis Manuel",
                    "lastName": "Guevara-Chumacero"
