            "itemType": "journalArticle",
            "title": "Energy metabolism and fasting in male and female insectivorous bats Molossus molossus (Chiroptera: Molossidae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Mb.",
                    "lastName": "Freitas"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ls.",
                    "lastName": "Goulart"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ms.",
                    "lastName": "Barros"
