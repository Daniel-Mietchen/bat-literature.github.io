            "key": "9K7EYGXU",
            "version": 56453,
            "itemType": "journalArticle",
            "title": "Landscape connectivity, habitat structure and activity of bat guilds in farmland‐dominated matrices",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Annie",
                    "lastName": "Frey‐Ehrenbold"
                },
                {
                    "creatorType": "author",
                    "firstName": "Fabio",
                    "lastName": "Bontadina"
                },
                {
                    "creatorType": "author",
                    "firstName": "Raphaël",
                    "lastName": "Arlettaz"
                },
