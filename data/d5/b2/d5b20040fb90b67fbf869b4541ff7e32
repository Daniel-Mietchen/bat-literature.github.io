            "key": "YIKUF2GT",
            "version": 17549,
            "itemType": "journalArticle",
            "title": "Increasing species richness along elevational gradients is associated with niche packing in bat assemblages",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Rohit",
                    "lastName": "Chakravarty"
                },
                {
                    "creatorType": "author",
                    "firstName": "Viktoriia",
                    "lastName": "Radchuk"
                },
                {
                    "creatorType": "author",
                    "firstName": "Shreyas",
                    "lastName": "Managave"
