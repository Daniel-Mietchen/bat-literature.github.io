            "key": "CEGY88H4",
            "version": 17913,
            "itemType": "journalArticle",
            "title": "Do We Need to Use Bats as Bioindicators?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Danilo",
                    "lastName": "Russo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Valeria B.",
                    "lastName": "Salinas-Ramos"
                },
                {
                    "creatorType": "author",
                    "firstName": "Luca",
