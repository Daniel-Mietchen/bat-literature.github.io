            "itemType": "journalArticle",
            "title": "Echolocation in the bat, <i>Rhinolophus capensis</i> : the influence of clutter, conspecifics and prey on call design and intensity",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Kayleigh",
                    "lastName": "Fawcett"
                },
                {
                    "creatorType": "author",
                    "firstName": "David S.",
                    "lastName": "Jacobs"
                },
