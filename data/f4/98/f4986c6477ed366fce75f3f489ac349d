            "version": 17915,
            "itemType": "journalArticle",
            "title": "Phylogeography and biogeography of the lower <span style=\"font-variant:small-caps;\">C</span> entral <span style=\"font-variant:small-caps;\">A</span> merican <span style=\"font-variant:small-caps;\">N</span> eotropics: diversification between two continents and between two seas",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Justin C.",
                    "lastName": "Bagley"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jerald B.",
                    "lastName": "Johnson"
