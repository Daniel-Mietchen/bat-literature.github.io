            "key": "5WAQQNYT",
            "version": 60670,
            "itemType": "journalArticle",
            "title": "Flight characteristics of birds: I. radar measurements of speeds",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Bruno",
                    "lastName": "Bruderer"
                },
