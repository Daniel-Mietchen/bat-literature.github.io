            "key": "4C7SM4R3",
            "version": 11581,
            "itemType": "journalArticle",
            "title": "Global Foot-and-Mouth Disease Research Update and Gap Analysis: 2 – Epidemiology, Wildlife and Economics",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "T. J. D.",
                    "lastName": "Knight‐Jones"
                },
                {
                    "creatorType": "author",
                    "firstName": "L.",
                    "lastName": "Robinson"
                },
                {
                    "creatorType": "author",
                    "firstName": "B.",
                    "lastName": "Charleston"
                },
                {
                    "creatorType": "author",
                    "firstName": "L. L.",
                    "lastName": "Rodriguez"
