            "version": 32562,
            "itemType": "journalArticle",
            "title": "Seasonal variation and host sex affect bat–bat fly interaction networks in the Amazonian savannahs",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Paulo",
                    "lastName": "Mejia"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gustavo Lima",
                    "lastName": "Urbieta"
                },
                {
                    "creatorType": "author",
                    "firstName": "Bruna Da Silva",
                    "lastName": "Xavier"
                },
                {
                    "creatorType": "author",
                    "firstName": "Isaí Jorge De",
                    "lastName": "Castro"
