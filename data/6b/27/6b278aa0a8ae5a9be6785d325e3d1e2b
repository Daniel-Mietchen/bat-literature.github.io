            "volume": "13",
            "issue": "7",
            "pages": "e0200255",
            "date": "2018-7-25",
            "series": "",
            "seriesTitle": "",
            "seriesText": "",
            "journalAbbreviation": "PLoS ONE",
            "language": "en",
