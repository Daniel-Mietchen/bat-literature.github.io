            "volume": "293",
            "issue": "3",
            "pages": "211-217",
            "date": "07/2014",
            "series": "",
            "seriesTitle": "",
            "seriesText": "",
            "journalAbbreviation": "Journal of Zoology",
            "language": "en",
            "DOI": "10.1111/jzo.12136",
            "ISSN": "0952-8369, 1469-7998",
