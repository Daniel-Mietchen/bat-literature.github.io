            "key": "D6ZRCBUZ",
            "version": 35028,
            "itemType": "journalArticle",
            "title": "Sex-specific roost selection by adult red bats in a diverse forested landscape",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Roger W.",
                    "lastName": "Perry"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ronald E.",
                    "lastName": "Thill"
                },
