            "key": "UZUYFWZS",
            "version": 58537,
            "itemType": "journalArticle",
            "title": "Local variability in the diet of Daubenton’s bat (Myotis daubentonii) in a lake landscape of northern Germany",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Henning",
                    "lastName": "Nissen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Frauke",
                    "lastName": "Krüger"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andreas",
                    "lastName": "Fichtner"
                },
