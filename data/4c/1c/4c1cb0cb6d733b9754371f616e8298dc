        "data": {
            "key": "E4DQE8PT",
            "version": 17547,
            "itemType": "journalArticle",
            "title": "Environmental conditions associated with bat white‐nose syndrome mortality in the north‐eastern United States",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Abigail R.",
                    "lastName": "Flory"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sunil",
                    "lastName": "Kumar"
                },
                {
                    "creatorType": "author",
                    "firstName": "Thomas J.",
                    "lastName": "Stohlgren"
