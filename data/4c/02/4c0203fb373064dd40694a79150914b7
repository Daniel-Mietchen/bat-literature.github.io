            "key": "7PR3DAPE",
            "version": 17106,
            "itemType": "journalArticle",
            "title": "Impact of Posttyphoon Hunting on Mariana Fruit Bats (Pteropus mariannus)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jacob A.",
                    "lastName": "Esselstyn"
                },
                {
                    "creatorType": "author",
                    "firstName": "Arjun",
                    "lastName": "Amar"
                },
