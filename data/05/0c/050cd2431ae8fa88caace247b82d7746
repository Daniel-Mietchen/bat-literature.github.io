            "version": 56438,
            "itemType": "journalArticle",
            "title": "Molecular Ecology of the Big Brown Bat (Eptesicus fuscus): Genetic and Natural History Variation in a Hybrid Zone",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Melissa A.",
                    "lastName": "Neubaum"
                },
                {
                    "creatorType": "author",
                    "firstName": "Marlis R.",
                    "lastName": "Douglas"
                },
                {
                    "creatorType": "author",
                    "firstName": "Michael E.",
