            "key": "JSY485CP",
            "version": 28907,
            "itemType": "journalArticle",
            "title": "Bee Markers: A Novel Method for Non-Invasive Short Term Marking of Bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Lucinda",
                    "lastName": "Kirkpatrick"
                },
                {
                    "creatorType": "author",
                    "firstName": "Grzegorz",
                    "lastName": "Apoznański"
                },
                {
                    "creatorType": "author",
                    "firstName": "Luc",
                    "lastName": "De Bruyn"
