            "version": 28878,
            "itemType": "journalArticle",
            "title": "Comparison of zoogeography among rats, fruit bats and insectivorous bats on Indonesian islands.",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ibnu",
                    "lastName": "Maryanto"
                },
                {
                    "creatorType": "author",
                    "firstName": "Seigo",
                    "lastName": "Higashi"
                }
