            "key": "G28VLBG2",
            "version": 28905,
            "itemType": "journalArticle",
            "title": "Parasitism by Bat Flies on an Urban Population of Cynopterus brachyotis in Singapore",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Vivien Naomi",
                    "lastName": "Lee"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ian H.",
                    "lastName": "Mendenhall"
                },
