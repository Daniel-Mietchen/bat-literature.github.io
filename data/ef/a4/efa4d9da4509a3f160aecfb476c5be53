            "version": 56243,
            "itemType": "journalArticle",
            "title": "Molecular phylogeography of Pteronotus davyi (Chiroptera: Mormoopidae) in Mexico",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Luis M.",
                    "lastName": "Guevara-Chumacero"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ricardo",
                    "lastName": "López-Wilchis"
                },
                {
                    "creatorType": "author",
                    "firstName": "Francisco F.",
                    "lastName": "Pedroche"
                },
                {
                    "creatorType": "author",
                    "firstName": "Javier",
                    "lastName": "Juste"
                },
                {
                    "creatorType": "author",
                    "firstName": "Carlos",
