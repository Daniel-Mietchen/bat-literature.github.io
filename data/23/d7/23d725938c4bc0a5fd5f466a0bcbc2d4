            "itemType": "journalArticle",
            "title": "Ethanol and Methanol as Possible Odor Cues for Egyptian Fruit Bats (Rousettus aegyptiacus)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Francisco",
                    "lastName": "Sánchez"
                },
                {
                    "creatorType": "author",
                    "firstName": "Carmi",
                    "lastName": "Korine"
                },
                {
                    "creatorType": "author",
                    "firstName": "Marco",
                    "lastName": "Steeghs"
                },
                {
                    "creatorType": "author",
                    "firstName": "Luc-Jan",
                    "lastName": "Laarhoven"
                },
                {
                    "creatorType": "author",
                    "firstName": "Simona M.",
                    "lastName": "Cristescu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Frans J. M.",
                    "lastName": "Harren"
                },
                {
                    "creatorType": "author",
                    "firstName": "Robert",
                    "lastName": "Dudley"
