            "version": 1409,
            "itemType": "journalArticle",
            "title": "Detection of new genetic variants of Betacoronaviruses in Endemic Frugivorous Bats of Madagascar",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Norosoa H.",
                    "lastName": "Razanajatovo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Lalaina A.",
                    "lastName": "Nomenjanahary"
                },
