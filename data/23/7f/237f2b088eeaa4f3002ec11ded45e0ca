            "key": "HKTN3Q2K",
            "version": 56351,
            "itemType": "journalArticle",
            "title": "Holocene and Late Pleistocene Bat Fossils (Mammalia: Chiroptera) from Hamilton County, TN, and their Ecological Implications",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Timothy J.",
                    "lastName": "Gaudin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ashley N.",
                    "lastName": "Miller"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jeremy L.",
                    "lastName": "Bramblett"
                },
