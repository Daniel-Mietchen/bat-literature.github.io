                    "creatorType": "author",
                    "firstName": "Nancy B.",
                    "lastName": "Simmons"
                },
                {
                    "creatorType": "author",
                    "firstName": "Natalia",
                    "lastName": "Ivanova"
                },
                {
                    "creatorType": "author",
                    "firstName": "Amanda",
                    "lastName": "Naaum"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andrew",
                    "lastName": "Briscoe"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rosie",
                    "lastName": "Drinkwater"
