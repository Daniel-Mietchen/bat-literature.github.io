        "data": {
            "key": "GB74V26Q",
            "version": 35250,
            "itemType": "journalArticle",
            "title": "Social foraging in vampire bats is predicted by long-term cooperative relationships",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Simon P.",
                    "lastName": "Ripperger"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gerald G.",
                    "lastName": "Carter"
                },
                {
                    "creatorType": "editor",
                    "firstName": "Catherine",
                    "lastName": "Hobaiter"
                }
