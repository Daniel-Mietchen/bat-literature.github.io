            "key": "XMCRCD4B",
            "version": 29382,
            "itemType": "journalArticle",
            "title": "Influence of Surface Area, Water Level and Adjacent Vegetation on Bat Use of Artificial Water Sources",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Sara L.",
                    "lastName": "Jackrel"
                },
                {
                    "creatorType": "author",
                    "firstName": "Raymond S.",
                    "lastName": "Matlack"
                }
