            "key": "T84IBSHC",
            "version": 59037,
            "itemType": "journalArticle",
            "title": "Synopsis and Biogeography of the Mammals of Camiguin Island, Philippines",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Lawrence R",
                    "lastName": "Heaney"
                },
                {
                    "creatorType": "author",
                    "firstName": "Blas R.",
                    "lastName": "Tabaranza Jr."
                },
                {
                    "creatorType": "author",
                    "firstName": "Danilo S.",
