        "data": {
            "key": "EW6RKX7A",
            "version": 56756,
            "itemType": "journalArticle",
            "title": "A Predictive Spatial Distribution Framework for Filovirus-Infected Bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "G.",
                    "lastName": "Fiorillo"
                },
                {
                    "creatorType": "author",
                    "firstName": "P.",
                    "lastName": "Bocchini"
