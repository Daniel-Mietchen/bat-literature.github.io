            "itemType": "journalArticle",
            "title": "Three‐dimensional stratification pattern in an old‐growth lowland forest: How does height in canopy and season influence temperate bat activity?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Maude",
                    "lastName": "Erasmy"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christoph",
                    "lastName": "Leuschner"
                },
                {
                    "creatorType": "author",
                    "firstName": "Niko",
