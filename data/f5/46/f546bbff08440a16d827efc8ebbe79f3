            "itemType": "journalArticle",
            "title": "The hierarchical radiation of phyllostomid bats as revealed by adaptive molar morphology",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "David M.",
                    "lastName": "Grossnickle"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alexa",
                    "lastName": "Sadier"
                },
                {
                    "creatorType": "author",
                    "firstName": "Edward",
                    "lastName": "Patterson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Nashaly N.",
                    "lastName": "Cortés-Viruet"
                },
                {
                    "creatorType": "author",
                    "firstName": "Stephanie M.",
                    "lastName": "Jiménez-Rivera"
                },
                {
                    "creatorType": "author",
                    "firstName": "Karen E.",
                    "lastName": "Sears"
                },
