            "version": 56313,
            "itemType": "journalArticle",
            "title": "Diversification of an emerging pathogen in a biodiversity hotspot: Leptospira in endemic small mammals of Madagascar",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Muriel",
                    "lastName": "Dietrich"
                },
                {
                    "creatorType": "author",
                    "firstName": "David A.",
                    "lastName": "Wilkinson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Voahangy",
                    "lastName": "Soarimalala"
                },
                {
                    "creatorType": "author",
                    "firstName": "Steven M.",
