            "itemType": "journalArticle",
            "title": "Plasma proteomic analysis of active and torpid greater mouse-eared bats (Myotis myotis)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Alexander M.",
                    "lastName": "Hecht"
                },
                {
                    "creatorType": "author",
                    "firstName": "Beate C.",
                    "lastName": "Braun"
                },
                {
                    "creatorType": "author",
                    "firstName": "Eberhard",
                    "lastName": "Krause"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christian C.",
                    "lastName": "Voigt"
