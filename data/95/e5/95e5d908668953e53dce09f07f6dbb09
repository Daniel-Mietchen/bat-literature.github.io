            "version": 32601,
            "itemType": "journalArticle",
            "title": "Patterns of Abundance and Host Specificity of Bat Ectoparasites in the Central Balkans",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "J",
                    "lastName": "Burazerović"
                },
                {
                    "creatorType": "author",
                    "firstName": "M",
                    "lastName": "Orlova"
                },
                {
                    "creatorType": "author",
                    "firstName": "M",
                    "lastName": "Obradović"
