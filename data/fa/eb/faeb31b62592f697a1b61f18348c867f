            "key": "LKMVAZFU",
            "version": 225,
            "itemType": "journalArticle",
            "title": "Evolutionary Dynamics of Pathogen Resistance and Tolerance",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "B. A.",
                    "lastName": "Roy"
                },
                {
                    "creatorType": "author",
                    "firstName": "J. W.",
                    "lastName": "Kirchner"
