            "version": 542,
            "itemType": "journalArticle",
            "title": "The B cell antigen receptor and overexpression of MYC can cooperate in the genesis of B cell lymphomas",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Y.",
                    "lastName": "Refaeli"
                },
                {
                    "creatorType": "author",
                    "firstName": "R.M.",
                    "lastName": "Young"
                },
                {
                    "creatorType": "author",
                    "firstName": "B.C.",
