            "itemType": "journalArticle",
            "title": "Nuclear introns outperform mitochondrial DNA in inter-specific phylogenetic reconstruction: Lessons from horseshoe bats (Rhinolophidae: Chiroptera)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Serena E.",
                    "lastName": "Dool"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sebastien J.",
                    "lastName": "Puechmaille"
                },
                {
                    "creatorType": "author",
                    "firstName": "Nicole M.",
                    "lastName": "Foley"
                },
                {
                    "creatorType": "author",
                    "firstName": "Benjamin",
                    "lastName": "Allegrini"
                },
                {
                    "creatorType": "author",
                    "firstName": "Anna",
                    "lastName": "Bastian"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gregory L.",
                    "lastName": "Mutumi"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tinyiko G.",
                    "lastName": "Maluleke"
                },
                {
                    "creatorType": "author",
                    "firstName": "Lizelle J.",
                    "lastName": "Odendaal"
