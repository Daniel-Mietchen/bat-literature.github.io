            "version": 57728,
            "itemType": "journalArticle",
            "title": "The ecological impact of city lighting scenarios: exploring gap crossing thresholds for urban bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "James D.",
                    "lastName": "Hale"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alison J.",
                    "lastName": "Fairbrass"
                },
                {
                    "creatorType": "author",
                    "firstName": "Thomas J.",
                    "lastName": "Matthews"
