            "key": "RF7H3PX6",
            "version": 78,
            "itemType": "journalArticle",
            "title": "Bats are key hosts in the radiation of mammal-associated Bartonella bacteria",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Clifton D.",
                    "lastName": "McKee"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ying",
                    "lastName": "Bai"
                },
                {
                    "creatorType": "author",
                    "firstName": "Colleen T.",
                    "lastName": "Webb"
                },
