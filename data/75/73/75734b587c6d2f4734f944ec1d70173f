            "version": 54198,
            "itemType": "journalArticle",
            "title": "Bats that walk: a new evolutionary hypothesis for the terrestrial behaviour of New Zealand's endemic mystacinids",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Suzanne J",
                    "lastName": "Hand"
                },
                {
                    "creatorType": "author",
                    "firstName": "Vera",
                    "lastName": "Weisbecker"
                },
                {
                    "creatorType": "author",
                    "firstName": "Robin Md",
                    "lastName": "Beck"
                },
                {
                    "creatorType": "author",
                    "firstName": "Michael",
                    "lastName": "Archer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Henk",
                    "lastName": "Godthelp"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alan Jd",
