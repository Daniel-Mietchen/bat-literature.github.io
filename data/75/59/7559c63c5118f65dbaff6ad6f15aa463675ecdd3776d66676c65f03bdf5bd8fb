            "key": "P3IFTRLY",
            "version": 1419,
            "itemType": "journalArticle",
            "title": "Molecular systematics and phylogeography of the tribe Myonycterini (Mammalia, Pteropodidae) inferred from mitochondrial and nuclear markers",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Nicolas",
                    "lastName": "Nesi"
                },
                {
                    "creatorType": "author",
                    "firstName": "Blaise",
                    "lastName": "Kadjo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Xavier",
                    "lastName": "Pourrut"
                },
                {
                    "creatorType": "author",
                    "firstName": "Eric",
                    "lastName": "Leroy"
                },
                {
                    "creatorType": "author",
                    "firstName": "Célestin",
                    "lastName": "Pongombo Shongo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Corinne",
                    "lastName": "Cruaud"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alexandre",
                    "lastName": "Hassanin"
