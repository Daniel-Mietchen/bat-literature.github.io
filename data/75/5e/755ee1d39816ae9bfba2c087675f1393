            "key": "AHJBRTZ8",
            "version": 38462,
            "itemType": "journalArticle",
            "title": "Variability in bat morphology is influenced by temperature and forest cover and their interactions",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Heather",
                    "lastName": "Wood"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sara A. O.",
                    "lastName": "Cousins"
                }
