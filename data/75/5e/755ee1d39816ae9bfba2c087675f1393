            "version": 38461,
            "itemType": "journalArticle",
            "title": "Seasonal swarming behavior of <i>Myotis</i> bats revealed by integrated monitoring, involving passive acoustic monitoring with automated analysis, trapping, and video monitoring",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Robert J.",
                    "lastName": "Thomas"
                },
                {
                    "creatorType": "author",
                    "firstName": "Stephen P.",
                    "lastName": "Davison"
                }
