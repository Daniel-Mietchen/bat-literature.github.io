                    "creatorType": "author",
                    "firstName": "Isabelle",
                    "lastName": "Le Viol"
                },
                {
                    "creatorType": "author",
                    "firstName": "Agathe",
                    "lastName": "Dumont"
                },
                {
                    "creatorType": "author",
                    "firstName": "Thomas",
                    "lastName": "Deana"
                },
                {
                    "creatorType": "author",
                    "firstName": "Stéphane",
                    "lastName": "Vincent"
                },
                {
                    "creatorType": "author",
                    "firstName": "Samuel",
                    "lastName": "Challéat"
