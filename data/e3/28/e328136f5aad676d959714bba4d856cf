            "key": "BMY4SETF",
            "version": 16701,
            "itemType": "journalArticle",
            "title": "White‐nose Syndrome and Environmental Correlates to Landscape‐Scale Bat Presence",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Elaine L.",
                    "lastName": "Barr"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alexander",
                    "lastName": "Silvis"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mike P.",
                    "lastName": "Armstrong"
                },
