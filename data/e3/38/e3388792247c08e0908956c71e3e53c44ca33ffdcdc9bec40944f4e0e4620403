            "key": "GG6AJUKQ",
            "version": 393,
            "itemType": "journalArticle",
            "title": "Muscle Non-shivering Thermogenesis and Its Role in the Evolution of Endothermy",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Julia",
                    "lastName": "Nowack"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sylvain",
                    "lastName": "Giroud"
                },
                {
                    "creatorType": "author",
                    "firstName": "Walter",
