            "version": 29174,
            "itemType": "journalArticle",
            "title": "Perch use by flycatching Rhinolophus formosae in relation to vegetation structure",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ya-Fu",
                    "lastName": "Lee"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yen-Min",
                    "lastName": "Kuo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Wen-Chen",
