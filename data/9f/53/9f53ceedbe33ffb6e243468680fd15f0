            "version": 46523,
            "itemType": "journalArticle",
            "title": "Mitochondrial Phylogeny and Morphological Variation of the African Sheath-Tailed Bat ( Coleura afra, Emballonuridae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Peter",
                    "lastName": "Vallo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Peggy",
                    "lastName": "Motsch"
                },
                {
                    "creatorType": "author",
                    "firstName": "Petr",
                    "lastName": "Benda"
