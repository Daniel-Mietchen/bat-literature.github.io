            "key": "FFWWKHAL",
            "version": 20,
            "itemType": "journalArticle",
            "title": "A phylogenetic supertree of the bats (Mammalia: Chiroptera)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Kate E.",
                    "lastName": "Jones"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andy",
                    "lastName": "Purvis"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ann",
                    "lastName": "MacLARNON"
                },
                {
                    "creatorType": "author",
                    "firstName": "Olaf R. P.",
