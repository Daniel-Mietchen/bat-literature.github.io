            "key": "UL3YGEN2",
            "version": 35284,
            "itemType": "journalArticle",
            "title": "Applying mobile acoustic surveys to model bat habitat use across sinuous routes",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Vanessa G.",
                    "lastName": "Rojas"
                },
                {
                    "creatorType": "author",
                    "firstName": "Susan C.",
                    "lastName": "Loeb"
                },
