            "version": 45706,
            "itemType": "journalArticle",
            "title": "DIFFERENTIAL RESOURCE USE BY PRIMATES AND HORNBILLS: IMPLICATIONS FOR SEED DISPERSAL",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "John R",
                    "lastName": "Poulsen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Connie J",
                    "lastName": "Clark"
                },
                {
                    "creatorType": "author",
                    "firstName": "Edward F",
