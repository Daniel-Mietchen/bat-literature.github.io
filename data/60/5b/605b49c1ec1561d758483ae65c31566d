            "version": 58475,
            "itemType": "journalArticle",
            "title": "INTERACTIONS BETWEEN ENVIRONMENT, SPECIES TRAITS, AND HUMAN USES DESCRIBE PATTERNS OF PLANT INVASIONS",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Wilfried",
                    "lastName": "Thuiller"
                },
                {
                    "creatorType": "author",
                    "firstName": "David M.",
                    "lastName": "Richardson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mathieu",
                    "lastName": "Rouget"
                },
                {
                    "creatorType": "author",
                    "firstName": "Şerban",
