            "key": "J966Q73A",
            "version": 32690,
            "itemType": "journalArticle",
            "title": "Quantifying and sustaining biodiversity in tropical agricultural landscapes",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Chase D.",
                    "lastName": "Mendenhall"
                },
                {
                    "creatorType": "author",
                    "firstName": "Analisa",
                    "lastName": "Shields-Estrada"
                },
                {
                    "creatorType": "author",
                    "firstName": "Arjun J.",
                    "lastName": "Krishnaswami"
                },
