            "key": "TUSXJQ3A",
            "version": 56175,
            "itemType": "journalArticle",
            "title": "Long-Term Survival of an Urban Fruit Bat Seropositive for Ebola and Lagos Bat Viruses",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "David T. S.",
                    "lastName": "Hayman"
                },
                {
                    "creatorType": "author",
                    "firstName": "Petra",
                    "lastName": "Emmerich"
                },
                {
                    "creatorType": "author",
                    "firstName": "Meng",
                    "lastName": "Yu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Lin-Fa",
                    "lastName": "Wang"
                },
                {
                    "creatorType": "author",
                    "firstName": "Richard",
                    "lastName": "Suu-Ire"
                },
                {
                    "creatorType": "author",
                    "firstName": "Anthony R.",
                    "lastName": "Fooks"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andrew A.",
