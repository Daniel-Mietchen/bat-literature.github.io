            "key": "9ILTAI4A",
            "version": 58280,
            "itemType": "journalArticle",
            "title": "Phylogenetic and geographic patterns of bartonella host shifts among bat species",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Clifton D.",
                    "lastName": "McKee"
                },
                {
                    "creatorType": "author",
                    "firstName": "David T. S.",
                    "lastName": "Hayman"
                },
