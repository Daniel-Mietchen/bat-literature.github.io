            "itemType": "journalArticle",
            "title": "Similar hibernation physiology in bats across broad geographic ranges",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Liam P.",
                    "lastName": "McGuire"
                },
                {
                    "creatorType": "author",
                    "firstName": "Nathan W.",
                    "lastName": "Fuller"
                },
