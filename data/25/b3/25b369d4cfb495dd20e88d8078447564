            "version": 58304,
            "itemType": "journalArticle",
            "title": "Diversity and Evolution of Viral Pathogen Community in Cave Nectar Bats (Eonycteris spelaea)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ian H.",
                    "lastName": "Mendenhall"
                },
                {
                    "creatorType": "author",
                    "firstName": "Dolyce Low Hong",
                    "lastName": "Wen"
                },
