            "key": "XXRYVTUP",
            "version": 47315,
            "itemType": "journalArticle",
            "title": "Marine and terrestrial food sources in the diet of the fish-eating myotis (Myotis vivesi)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Aída",
                    "lastName": "Otálora-Ardila"
                },
                {
                    "creatorType": "author",
                    "firstName": "L. Gerardo",
                    "lastName": "Herrera M."
                },
                {
                    "creatorType": "author",
                    "firstName": "José Juan",
