        "data": {
            "key": "3FBN9522",
            "version": 393,
            "itemType": "journalArticle",
            "title": "Mutual mother-pup acoustic identification in Asian particolored bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Xiao",
                    "lastName": "Tan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yu",
                    "lastName": "Li"
                },
                {
                    "creatorType": "author",
                    "firstName": "Keping",
                    "lastName": "Sun"
                },
