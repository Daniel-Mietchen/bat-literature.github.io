            "version": 57116,
            "itemType": "journalArticle",
            "title": "Molecular diet analysis finds an insectivorous desert bat community dominated by resource sharing despite diverse echolocation and foraging strategies",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Rowena",
                    "lastName": "Gordon"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sally",
                    "lastName": "Ivens"
                },
                {
                    "creatorType": "author",
                    "firstName": "Loren K.",
                    "lastName": "Ammerman"
                },
                {
                    "creatorType": "author",
                    "firstName": "M. Brock",
                    "lastName": "Fenton"
