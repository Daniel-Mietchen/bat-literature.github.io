            "version": 12185,
            "itemType": "journalArticle",
            "title": "Molecular diet analysis of neotropical bats based on fecal DNA metabarcoding",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Melissa R.",
                    "lastName": "Ingala"
                },
                {
                    "creatorType": "author",
                    "firstName": "Nancy B.",
                    "lastName": "Simmons"
                },
                {
                    "creatorType": "author",
                    "firstName": "Claudia",
                    "lastName": "Wultsch"
