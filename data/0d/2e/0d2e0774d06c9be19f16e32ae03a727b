            "version": 29328,
            "itemType": "journalArticle",
            "title": "Using Approximate Bayesian Computation to infer sex ratios from acoustic data",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Lisa",
                    "lastName": "Lehnen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Wigbert",
                    "lastName": "Schorcht"
