            "key": "CHZ2AM46",
            "version": 45629,
            "itemType": "journalArticle",
            "title": "Food Intake Changes in Relation to Food Quality in the Neotropical Frugivorous Bat <i>Sturnira ludovici</i>",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Romeo A.",
                    "lastName": "Saldaña-Vázquez"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jorge E.",
                    "lastName": "Schondube"
                }
