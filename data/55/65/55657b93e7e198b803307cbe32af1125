            "itemType": "journalArticle",
            "title": "Bat Predation by Spiders",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Martin",
                    "lastName": "Nyffeler"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mirjam",
