            "key": "MD9U93V8",
            "version": 13789,
            "itemType": "journalArticle",
            "title": "Sensitivity of insectivorous bats to urbanization: Implications for suburban conservation planning",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Caragh G.",
                    "lastName": "Threlfall"
                },
                {
                    "creatorType": "author",
                    "firstName": "Bradley",
                    "lastName": "Law"
                },
