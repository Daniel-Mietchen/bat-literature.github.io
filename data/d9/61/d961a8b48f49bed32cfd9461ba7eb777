            "key": "M9ECKMXP",
            "version": 42524,
            "itemType": "journalArticle",
            "title": "Current and potential distribution of Myotis simus (Chiroptera, Vespertilionidae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ricardo",
                    "lastName": "Moratelli"
                },
                {
                    "creatorType": "author",
                    "firstName": "Cecilia S.",
                    "lastName": "De Andreazzi"
                },
                {
                    "creatorType": "author",
