            "key": "KZ9YSHHA",
            "version": 39470,
            "itemType": "journalArticle",
            "title": "Greater Horseshoe Bats Recognize the Sex and Individual Identity of Conspecifics from Their Echolocation Calls",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Xiao",
                    "lastName": "Tan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Aiqing",
                    "lastName": "Lin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Keping",
                    "lastName": "Sun"
                },
