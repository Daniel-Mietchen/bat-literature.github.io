            "itemType": "journalArticle",
            "title": "Natural and anthropogenic factors influencing the bat community in commercial tree stands in a temperate lowland forest of natural origin (Białowieża Forest)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Alek",
                    "lastName": "Rachwald"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jan S.",
                    "lastName": "Boratyński"
                },
                {
                    "creatorType": "author",
                    "firstName": "Justyna",
