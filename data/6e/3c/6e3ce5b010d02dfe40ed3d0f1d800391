            "itemType": "journalArticle",
            "title": "Bats experience age-related hearing loss (presbycusis)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Yifat Chaya",
                    "lastName": "Tarnovsky"
                },
                {
                    "creatorType": "author",
                    "firstName": "Shahar",
                    "lastName": "Taiber"
