            "version": 37747,
            "itemType": "journalArticle",
            "title": "The components of biodiversity, with a particular focus on phylogenetic information",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Samuel M.",
                    "lastName": "Scheiner"
                },
                {
                    "creatorType": "author",
                    "firstName": "Evsey",
                    "lastName": "Kosman"
                },
                {
                    "creatorType": "author",
                    "firstName": "Steven J.",
                    "lastName": "Presley"
