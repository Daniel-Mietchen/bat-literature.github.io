            "version": 37749,
            "itemType": "journalArticle",
            "title": "Little brown bat activity patterns and conservation implications in agricultural landscapes in boreal Yukon, Canada",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Brian G.",
                    "lastName": "Slough"
                },
                {
                    "creatorType": "author",
                    "firstName": "Donald G.",
                    "lastName": "Reid"
                },
                {
                    "creatorType": "author",
                    "firstName": "Dafna S.",
