            "key": "CLQFKNFN",
            "version": 37678,
            "itemType": "journalArticle",
            "title": "Detecting Bat Calls: An Analysis of Automated Methods",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Mark D.",
                    "lastName": "Skowronski"
                },
                {
                    "creatorType": "author",
                    "firstName": "M.",
                    "lastName": "Brock Fenton"
                }
