            "key": "E984PZCC",
            "version": 781,
            "itemType": "journalArticle",
            "title": "Staphylococcus aureus Complex in the Straw-Colored Fruit Bat (Eidolon helvum) in Nigeria",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ayodele",
                    "lastName": "Olatimehin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Adebayo O.",
                    "lastName": "Shittu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Francis C.",
                    "lastName": "Onwugamba"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alexander",
                    "lastName": "Mellmann"
                },
                {
                    "creatorType": "author",
                    "firstName": "Karsten",
                    "lastName": "Becker"
                },
                {
                    "creatorType": "author",
                    "firstName": "Frieder",
                    "lastName": "Schaumburg"
                }
