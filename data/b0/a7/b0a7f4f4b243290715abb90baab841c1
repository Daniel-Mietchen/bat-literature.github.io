            "itemType": "journalArticle",
            "title": "Screening of Active Lyssavirus Infection in Wild Bat Populations by Viral RNA Detection on Oropharyngeal Swabs",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Juan E.",
                    "lastName": "Echevarrı́a"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ana",
                    "lastName": "Avellón"
