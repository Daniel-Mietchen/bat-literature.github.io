            "key": "PLRSPULX",
            "version": 29130,
            "itemType": "journalArticle",
            "title": "Flexible energy-saving strategies in female temperate-zone bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Lara",
                    "lastName": "Keicher"
                },
                {
                    "creatorType": "author",
                    "firstName": "J. Ryan",
                    "lastName": "Shipley"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ewa",
                    "lastName": "Komar"
