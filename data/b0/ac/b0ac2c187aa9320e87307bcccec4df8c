            "key": "YWLUVNE9",
            "version": 29118,
            "itemType": "journalArticle",
            "title": "Relative energy production determines effect of repowering on wildlife mortality at wind energy facilities",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Manuela",
                    "lastName": "Huso"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tara",
                    "lastName": "Conkling"
                },
                {
                    "creatorType": "author",
                    "firstName": "Daniel",
                    "lastName": "Dalthorp"
                },
                {
                    "creatorType": "author",
                    "firstName": "Melanie",
                    "lastName": "Davis"
                },
                {
                    "creatorType": "author",
                    "firstName": "Heath",
                    "lastName": "Smith"
                },
                {
                    "creatorType": "author",
                    "firstName": "Amy",
                    "lastName": "Fesnock"
