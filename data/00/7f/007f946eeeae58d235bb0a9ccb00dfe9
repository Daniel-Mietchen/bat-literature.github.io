            "key": "RSLGMUKF",
            "version": 17075,
            "itemType": "journalArticle",
            "title": "Bat activity is related to habitat structure and time since prescribed fire in managed pine barrens in New England",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Natalie",
                    "lastName": "Kay"
                },
                {
                    "creatorType": "author",
                    "firstName": "Amelia",
                    "lastName": "Sadlon"
                },
