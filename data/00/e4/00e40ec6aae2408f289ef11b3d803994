            "version": 48772,
            "itemType": "journalArticle",
            "title": "Inter- and intraspecific comparisons of retention time in insectivorous bat species (Vespertilionidae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "A.",
                    "lastName": "Roswag"
                },
                {
                    "creatorType": "author",
                    "firstName": "N. I.",
