            "version": 57404,
            "itemType": "journalArticle",
            "title": "Ectoparasitic Assemblages on Mormoopid Bats (Chiroptera: Mormoopidae) from Puerto Rico",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Allen",
                    "lastName": "Kurta"
                },
                {
                    "creatorType": "author",
                    "firstName": "John O",
                    "lastName": "Whitaker"
                },
                {
                    "creatorType": "author",
                    "firstName": "William J",
