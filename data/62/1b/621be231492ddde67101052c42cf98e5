            "key": "TY7TNWD7",
            "version": 17646,
            "itemType": "journalArticle",
            "title": "Syntactic properties of male courtship song in the lesser short-tailed bat, Mystacina tuberculata",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Kathleen",
                    "lastName": "Collier"
                },
                {
                    "creatorType": "author",
                    "firstName": "Stuart",
                    "lastName": "Parsons"
                }
