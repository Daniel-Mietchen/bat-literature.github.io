            "key": "HK5ZSJCH",
            "version": 1415,
            "itemType": "journalArticle",
            "title": "Host and viral traits predict zoonotic spillover from mammals",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Kevin J.",
                    "lastName": "Olival"
                },
                {
                    "creatorType": "author",
                    "firstName": "Parviez R.",
                    "lastName": "Hosseini"
                },
                {
                    "creatorType": "author",
                    "firstName": "Carlos",
                    "lastName": "Zambrana-Torrelio"
                },
                {
                    "creatorType": "author",
                    "firstName": "Noam",
                    "lastName": "Ross"
