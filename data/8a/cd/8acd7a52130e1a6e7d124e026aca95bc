            "version": 35264,
            "itemType": "journalArticle",
            "title": "Field Identification of Myotis yumanensis and Myotis lucifugus: A Morphological Evaluation",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Thomas J.",
                    "lastName": "Rodhouse"
                },
                {
                    "creatorType": "author",
                    "firstName": "Shonene A.",
