            "version": 35277,
            "itemType": "journalArticle",
            "title": "Changes in membrane carbohydrates distribution associated to epididymal sperm maturation during the prolonged sperm storage period of Corynorhinus mexicanus bat (Chiroptera: Vespertilionidae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ahiezer",
                    "lastName": "Rodríguez-Tobón"
                },
                {
                    "creatorType": "author",
                    "firstName": "Reyna",
                    "lastName": "Fierro"
                },
                {
                    "creatorType": "author",
                    "firstName": "Miguel Angel",
                    "lastName": "León-Galván"
                },
                {
                    "creatorType": "author",
                    "firstName": "Adolfo",
                    "lastName": "Rosado"
