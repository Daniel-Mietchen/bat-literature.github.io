            "itemType": "journalArticle",
            "title": "OPERATIONAL TONGUE LENGTH IN PHYLLOSTOMID NECTAR-FEEDING BATS",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "York",
                    "lastName": "Winter"
                },
                {
                    "creatorType": "author",
                    "firstName": "Otto",
                    "lastName": "Von Helversen"
