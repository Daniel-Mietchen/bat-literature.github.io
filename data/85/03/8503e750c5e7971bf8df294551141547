            "version": 11607,
            "itemType": "journalArticle",
            "title": "Empirical assessment of analysis workflows for differential expression analysis of human samples using RNA-Seq",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Claire R.",
                    "lastName": "Williams"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alyssa",
                    "lastName": "Baccarella"
                },
