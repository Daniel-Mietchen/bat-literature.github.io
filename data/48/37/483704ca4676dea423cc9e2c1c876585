            "itemType": "journalArticle",
            "title": "Nonlinear Distribution Pattern of Hibernating Bats in Caves along an Elevational Gradient in Mountain (Carpathians, Southern Poland)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Krzysztof",
                    "lastName": "Piksa"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jakub",
                    "lastName": "Nowak"
                },
                {
                    "creatorType": "author",
                    "firstName": "Michał",
