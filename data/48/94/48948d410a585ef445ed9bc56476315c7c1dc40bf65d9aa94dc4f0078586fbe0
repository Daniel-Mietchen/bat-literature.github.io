            "version": 1439,
            "itemType": "journalArticle",
            "title": "High diversity of West African bat malaria parasites and a tight link with rodent Plasmodium taxa",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Juliane",
                    "lastName": "Schaer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Susan L",
                    "lastName": "Perkins"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jan",
                    "lastName": "Decher"
                },
                {
                    "creatorType": "author",
                    "firstName": "Fabian H",
                    "lastName": "Leendertz"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jakob",
