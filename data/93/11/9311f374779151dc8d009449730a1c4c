            "version": 39804,
            "itemType": "journalArticle",
            "title": "Morphological variation of the female reproductive organs of the bat Artibeus lituratus during its different reproductive phases",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Alderrosy F.",
                    "lastName": "Rodrigues"
                },
                {
                    "creatorType": "author",
                    "firstName": "Cornélio S.",
                    "lastName": "Santiago"
                },
                {
                    "creatorType": "author",
                    "firstName": "Eliana",
