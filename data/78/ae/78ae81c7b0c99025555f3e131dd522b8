            "version": 17647,
            "itemType": "journalArticle",
            "title": "Phylogenetic Patterns in Mouth Posture and Echolocation Emission Behavior of Phyllostomid Bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Gloria",
                    "lastName": "Gessinger"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rachel",
                    "lastName": "Page"
                },
                {
                    "creatorType": "author",
                    "firstName": "Lena",
                    "lastName": "Wilfert"
                },
                {
                    "creatorType": "author",
                    "firstName": "Annemarie",
                    "lastName": "Surlykke"
