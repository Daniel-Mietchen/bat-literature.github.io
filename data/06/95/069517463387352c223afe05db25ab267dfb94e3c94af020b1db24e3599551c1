            "itemType": "journalArticle",
            "title": "Human monkeypox: an emerging zoonotic disease",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "S.",
                    "lastName": "Parker"
                },
                {
                    "creatorType": "author",
                    "firstName": "A.",
                    "lastName": "Nuara"
                },
