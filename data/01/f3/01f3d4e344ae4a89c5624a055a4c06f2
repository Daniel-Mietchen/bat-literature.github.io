            "itemType": "journalArticle",
            "title": "Dry season camps of flying‐foxes ( <i>Pteropus</i> spp.) in Kakadu World Heritage Area, north Australia",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Christopher R.",
                    "lastName": "Tidemann"
                },
                {
                    "creatorType": "author",
                    "firstName": "Michael J.",
                    "lastName": "Vardon"
                },
