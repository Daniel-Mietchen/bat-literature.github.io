            "key": "TFNCZCEL",
            "version": 17727,
            "itemType": "journalArticle",
            "title": "Behavioral patterns of bats at a wind turbine confirm seasonality of fatality risk",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Shifra Z.",
                    "lastName": "Goldenberg"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paul M.",
                    "lastName": "Cryan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paulo Marcos",
                    "lastName": "Gorresen"
