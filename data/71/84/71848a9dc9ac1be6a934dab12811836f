            "key": "TGCIVZAY",
            "version": 11576,
            "itemType": "journalArticle",
            "title": "Impacts of acute and long-term vehicle exposure on physiology and reproductive success of the northern spotted owl",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Lisa S.",
                    "lastName": "Hayward"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ann E.",
                    "lastName": "Bowles"
                },
                {
                    "creatorType": "author",
                    "firstName": "James C.",
