            "version": 38418,
            "itemType": "journalArticle",
            "title": "Dietary Composition of Four Common Chiropteran Species in a Bottomland Hardwood Forest",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Carla J.",
                    "lastName": "Weinkauf"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christopher E.",
                    "lastName": "Comer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Warren C.",
