            "version": 38438,
            "itemType": "journalArticle",
            "title": "Using occupancy estimation to assess the effectiveness of a regional multiple-species conservation plan: Bats in the Pacific Northwest",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Theodore J.",
                    "lastName": "Weller"
                }
