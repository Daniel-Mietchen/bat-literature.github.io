            "version": 47651,
            "itemType": "journalArticle",
            "title": "Winter Energetics of Female Indiana Bats Myotis sodalis*",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Katie M.",
                    "lastName": "Day"
                },
                {
                    "creatorType": "author",
                    "firstName": "Thomas E.",
                    "lastName": "Tomasi"
                }
