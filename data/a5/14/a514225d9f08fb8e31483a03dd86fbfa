            "key": "LQNGU4GY",
            "version": 47774,
            "itemType": "journalArticle",
            "title": "Matched Behavioral and Neural Adaptations for Low Sound Level Echolocation in a Gleaning Bat, Antrozous pallidus",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Kevin R.",
                    "lastName": "Measor"
                },
                {
                    "creatorType": "author",
                    "firstName": "Brian C.",
                    "lastName": "Leavell"
                },
                {
                    "creatorType": "author",
                    "firstName": "Dustin H.",
                    "lastName": "Brewton"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jeffrey",
                    "lastName": "Rumschlag"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jesse R.",
                    "lastName": "Barber"
                },
