            "key": "FCJTEXPC",
            "version": 13636,
            "itemType": "journalArticle",
            "title": "Landscape heterogeneity as an ecological filter of species traits",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Rémi",
                    "lastName": "Duflot"
                },
                {
                    "creatorType": "author",
                    "firstName": "Romain",
                    "lastName": "Georges"
                },
                {
                    "creatorType": "author",
                    "firstName": "Aude",
                    "lastName": "Ernoult"
