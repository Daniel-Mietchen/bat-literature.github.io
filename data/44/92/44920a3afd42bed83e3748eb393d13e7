            "itemType": "journalArticle",
            "title": "Individual and Temporal Variation in Outcrossing Rates and Pollen Flow Patterns in Ceiba pentandra (Malvaceae: Bombacoidea)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jorge",
                    "lastName": "Lobo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Silvia",
                    "lastName": "Solís"
                },
                {
                    "creatorType": "author",
                    "firstName": "Eric J.",
