            "key": "U5FUHXIP",
            "version": 47976,
            "itemType": "journalArticle",
            "title": "The hearing gene Prestin reunites echolocating bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Gang",
                    "lastName": "Li"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jinhong",
                    "lastName": "Wang"
                },
                {
                    "creatorType": "author",
                    "firstName": "Stephen J.",
                    "lastName": "Rossiter"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gareth",
                    "lastName": "Jones"
                },
                {
                    "creatorType": "author",
                    "firstName": "James A.",
                    "lastName": "Cotton"
                },
