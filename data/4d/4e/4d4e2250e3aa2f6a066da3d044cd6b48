            "key": "GZJCZLRB",
            "version": 17000,
            "itemType": "journalArticle",
            "title": "Interdisciplinary approaches to understanding disease emergence: The past, present, and future drivers of Nipah virus emergence",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Peter",
                    "lastName": "Daszak"
                },
                {
                    "creatorType": "author",
                    "firstName": "Carlos",
                    "lastName": "Zambrana-Torrelio"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tiffany L.",
                    "lastName": "Bogich"
                },
                {
                    "creatorType": "author",
                    "firstName": "Miguel",
                    "lastName": "Fernandez"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jonathan H.",
                    "lastName": "Epstein"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kris A.",
                    "lastName": "Murray"
                },
