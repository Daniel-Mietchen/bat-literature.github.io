            "volume": "",
            "issue": "",
            "pages": "",
            "date": "2023-09-27",
            "series": "",
            "seriesTitle": "",
            "seriesText": "",
            "journalAbbreviation": "BIO",
            "language": "en",
            "DOI": "10.11648/j.bio.20231105.11",
