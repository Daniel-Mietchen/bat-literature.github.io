            "version": 7557,
            "itemType": "journalArticle",
            "title": "Quantitative comparison of tree roosts used by red bats (Lasiurus borealis) and Seminole bats (L. seminolus)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Michael A",
                    "lastName": "Menzel"
                },
                {
                    "creatorType": "author",
                    "firstName": "Timothy C",
                    "lastName": "Carter"
                },
                {
                    "creatorType": "author",
                    "firstName": "Brian R",
                    "lastName": "Chapman"
                },
