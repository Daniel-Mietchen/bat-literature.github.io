                    "creatorType": "author",
                    "firstName": "Patricia A.",
                    "lastName": "Leman"
                },
                {
                    "creatorType": "author",
                    "firstName": "Antoinette A.",
                    "lastName": "Grobbelaar"
                },
                {
                    "creatorType": "author",
                    "firstName": "Monica",
                    "lastName": "Birkhead"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sarah",
                    "lastName": "Clift"
                },
                {
                    "creatorType": "author",
                    "firstName": "Robert",
                    "lastName": "Swanepoel"
