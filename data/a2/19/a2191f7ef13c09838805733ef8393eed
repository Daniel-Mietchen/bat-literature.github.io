            "version": 56476,
            "itemType": "journalArticle",
            "title": "Rapidly declining body size in an insectivorous bat is associated with increased precipitation and decreased survival",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Christina M.",
                    "lastName": "Davy"
                },
                {
                    "creatorType": "author",
                    "firstName": "Valerie",
                    "lastName": "Von Zuben"
                },
                {
                    "creatorType": "author",
                    "firstName": "Piia M.",
                    "lastName": "Kukka"
                },
                {
                    "creatorType": "author",
                    "firstName": "Brian D.",
                    "lastName": "Gerber"
