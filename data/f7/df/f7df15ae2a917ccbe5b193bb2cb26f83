            "version": 17889,
            "itemType": "journalArticle",
            "title": "Does sex matter? Gender‐specific responses to forest fragmentation in Neotropical bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ricardo",
                    "lastName": "Rocha"
                },
                {
                    "creatorType": "author",
                    "firstName": "Diogo F.",
                    "lastName": "Ferreira"
                },
                {
                    "creatorType": "author",
                    "firstName": "Adrià",
                    "lastName": "López‐Baucells"
                },
                {
                    "creatorType": "author",
                    "firstName": "Fábio Z.",
                    "lastName": "Farneda"
                },
                {
                    "creatorType": "author",
                    "firstName": "João M. B.",
                    "lastName": "Carreiras"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jorge M.",
                    "lastName": "Palmeirim"
