            "key": "FSQIF64Q",
            "version": 797,
            "itemType": "journalArticle",
            "title": "Can Bats Serve as Reservoirs for Arboviruses?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Anna C.",
                    "lastName": "Fagre"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rebekah C.",
                    "lastName": "Kading"
