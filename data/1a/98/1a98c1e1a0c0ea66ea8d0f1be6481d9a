                {
                    "creatorType": "author",
                    "firstName": "Brian R",
                    "lastName": "Amman"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mariano",
                    "lastName": "Sanchez-Lockhart"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jonathan S",
                    "lastName": "Towner"
