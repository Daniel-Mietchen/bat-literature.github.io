            "version": 56451,
            "itemType": "journalArticle",
            "title": "Contrasting patterns of elevational zonation for birds and mammals in the Andes of southeastern Peru",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Bruce D.",
                    "lastName": "Patterson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Douglas F.",
                    "lastName": "Stotz"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sergio",
                    "lastName": "Solari"
                },
                {
                    "creatorType": "author",
                    "firstName": "John W.",
                    "lastName": "Fitzpatrick"
