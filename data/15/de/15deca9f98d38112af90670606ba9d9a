            "key": "JLBEQ4VX",
            "version": 16763,
            "itemType": "journalArticle",
            "title": "Bat species richness and activity in Bou Hedma National Park (central Tunisia)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ridha",
                    "lastName": "Dalhoumi"
                },
                {
                    "creatorType": "author",
