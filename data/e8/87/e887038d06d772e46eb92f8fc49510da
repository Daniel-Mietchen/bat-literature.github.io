            "version": 35056,
            "itemType": "journalArticle",
            "title": "Bats as ecosystem engineers in iron ore caves in the Carajás National Forest, Brazilian Amazonia",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Luis B.",
                    "lastName": "Piló"
                },
                {
                    "creatorType": "author",
                    "firstName": "Allan",
                    "lastName": "Calux"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rafael",
