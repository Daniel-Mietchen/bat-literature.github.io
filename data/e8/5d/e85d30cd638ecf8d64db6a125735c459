            "version": 28990,
            "itemType": "journalArticle",
            "title": "Predator-Prey Relationship between Urban Bats and Insects Impacted by Both Artificial Light at Night and Spatial Clutter",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Han",
                    "lastName": "Li"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kenneth T.",
                    "lastName": "Wilkins"
                }
