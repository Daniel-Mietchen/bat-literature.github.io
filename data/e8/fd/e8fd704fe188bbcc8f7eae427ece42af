            "version": 38473,
            "itemType": "journalArticle",
            "title": "Diversity in olfactory receptor repertoires is associated with dietary specialization in a genus of frugivorous bat",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Laurel R",
                    "lastName": "Yohe"
                },
                {
                    "creatorType": "author",
                    "firstName": "Leith B",
                    "lastName": "Leiser-Miller"
                },
                {
                    "creatorType": "author",
                    "firstName": "Zofia A",
                    "lastName": "Kaliszewska"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paul",
                    "lastName": "Donat"
                },
