            "key": "XAPEPKIV",
            "version": 15429,
            "itemType": "journalArticle",
            "title": "Reproductive Phenology of Bat Assemblages in Vietnamese Karst and Its Conservation Implications",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Neil M.",
                    "lastName": "Furey"
                },
                {
                    "creatorType": "author",
                    "firstName": "Iain J.",
                    "lastName": "Mackie"
