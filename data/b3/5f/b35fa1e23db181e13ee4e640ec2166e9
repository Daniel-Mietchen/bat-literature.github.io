            "version": 56524,
            "itemType": "journalArticle",
            "title": "Phenotypic Convergence in Genetically Distinct Lineages of a Rhinolophus Species Complex (Mammalia, Chiroptera)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "David S.",
                    "lastName": "Jacobs"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hassan",
                    "lastName": "Babiker"
                },
                {
                    "creatorType": "author",
                    "firstName": "Anna",
                    "lastName": "Bastian"
