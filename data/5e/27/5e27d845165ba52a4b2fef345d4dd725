            "key": "KSN6R9R3",
            "version": 17306,
            "itemType": "journalArticle",
            "title": "Metabolic cost of acute phase response in the frugivorous bat, Artibeus lituratus",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Adriana L.",
                    "lastName": "Guerrero-Chacón"
                },
                {
                    "creatorType": "author",
                    "firstName": "David",
                    "lastName": "Rivera-Ruíz"
                },
