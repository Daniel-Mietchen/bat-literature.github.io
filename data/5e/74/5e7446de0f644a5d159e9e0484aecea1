            "version": 29280,
            "itemType": "journalArticle",
            "title": "Concluding Remarks: What Do We Need To Know About Bats in Northwestern North America?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Thomas S",
                    "lastName": "Jung"
                },
                {
                    "creatorType": "author",
                    "firstName": "Karen M",
                    "lastName": "Blejwas"
                },
                {
                    "creatorType": "author",
                    "firstName": "Cori L",
                    "lastName": "Lausen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Joanna M",
