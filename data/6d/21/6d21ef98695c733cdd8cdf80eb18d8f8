            "version": 29047,
            "itemType": "journalArticle",
            "title": "Hunting alters seedling functional trait composition in a Neotropical forest",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Erin L.",
                    "lastName": "Kurten"
                },
                {
                    "creatorType": "author",
                    "firstName": "S. Joseph",
                    "lastName": "Wright"
