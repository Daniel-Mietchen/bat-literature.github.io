            "version": 29046,
            "itemType": "journalArticle",
            "title": "Sweep Samples of Tropical Foliage Insects: Effects of Seasons, Vegetation Types, Elevation, Time of Day, and Insularity",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Daniel H.",
                    "lastName": "Janzen"
                }
