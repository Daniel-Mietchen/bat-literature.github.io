            "key": "LD4HMXMB",
            "version": 34995,
            "itemType": "journalArticle",
            "title": "The simulated effects of timber harvest on suitable habitat for Indiana and northern long‐eared bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "B. P.",
                    "lastName": "Pauli"
                },
                {
                    "creatorType": "author",
                    "firstName": "P. A.",
                    "lastName": "Zollner"
                },
                {
                    "creatorType": "author",
                    "firstName": "G. S.",
                    "lastName": "Haulton"
                },
                {
                    "creatorType": "author",
                    "firstName": "G.",
