                {
                    "creatorType": "author",
                    "firstName": "Carlos",
                    "lastName": "Ibañez"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gerald",
                    "lastName": "Kerth"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ana",
                    "lastName": "Popa‐Lisseanu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Wigbert",
