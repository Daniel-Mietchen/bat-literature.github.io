            "version": 1385,
            "itemType": "journalArticle",
            "title": "Serological evidence of exposure to ebolaviruses in domestic pigs from Guinea",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Kerstin",
                    "lastName": "Fischer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alimou",
                    "lastName": "Camara"
                },
                {
                    "creatorType": "author",
                    "firstName": "Cécile",
                    "lastName": "Troupin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sarah K.",
                    "lastName": "Fehling"
