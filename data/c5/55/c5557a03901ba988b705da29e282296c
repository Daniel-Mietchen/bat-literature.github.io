            "key": "4WB37YWT",
            "version": 43876,
            "itemType": "journalArticle",
            "title": "Bats as potential suppressors of multiple agricultural pests: A case study from Madagascar",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "James",
                    "lastName": "Kemp"
                },
                {
                    "creatorType": "author",
                    "firstName": "Adrià",
                    "lastName": "López-Baucells"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ricardo",
                    "lastName": "Rocha"
                },
                {
                    "creatorType": "author",
                    "firstName": "Owen S.",
                    "lastName": "Wangensteen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Zo",
                    "lastName": "Andriatafika"
                },
                {
                    "creatorType": "author",
                    "firstName": "Abhilash",
