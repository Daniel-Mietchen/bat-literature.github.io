            "key": "JFS6ETQB",
            "version": 13603,
            "itemType": "journalArticle",
            "title": "A meta‚Äêanalysis of human disturbance impacts on Antarctic wildlife",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Bernard W. T.",
                    "lastName": "Coetzee"
                },
                {
                    "creatorType": "author",
                    "firstName": "Steven L.",
                    "lastName": "Chown"
                }
