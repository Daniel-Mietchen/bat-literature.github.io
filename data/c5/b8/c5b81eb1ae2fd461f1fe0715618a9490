            "key": "K2KWFPQG",
            "version": 16895,
            "itemType": "journalArticle",
            "title": "Review: Abandoned mines as a resource or liability for wildlife",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Alejandro",
                    "lastName": "Grajal-Puche"
                },
