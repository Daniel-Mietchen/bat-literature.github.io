            "version": 460,
            "itemType": "journalArticle",
            "title": "Rabies in a captive colony of big brown bats (Eptesicus fuscus)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Vidya",
                    "lastName": "Shankar"
                },
                {
                    "creatorType": "author",
                    "firstName": "Richard A.",
                    "lastName": "Bowen"
                },
                {
                    "creatorType": "author",
                    "firstName": "April D.",
                    "lastName": "Davis"
                },
