                {
                    "creatorType": "author",
                    "firstName": "Seán B. A.",
                    "lastName": "Kelly"
                },
                {
                    "creatorType": "author",
                    "firstName": "Deirdre",
                    "lastName": "McClean"
                },
                {
                    "creatorType": "author",
                    "firstName": "David J.",
