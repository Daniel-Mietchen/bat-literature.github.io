            "version": 11584,
            "itemType": "journalArticle",
            "title": "Stealth echolocation in aerial hawking bats reflects a substrate gleaning ancestry",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Daniel",
                    "lastName": "Lewanzik"
                },
                {
                    "creatorType": "author",
                    "firstName": "John M.",
                    "lastName": "Ratcliffe"
                },
                {
                    "creatorType": "author",
                    "firstName": "Erik A.",
                    "lastName": "Etzler"
