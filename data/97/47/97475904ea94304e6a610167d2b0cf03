            "key": "N6SN38P8",
            "version": 56233,
            "itemType": "journalArticle",
            "title": "Bat Predation by Spiders",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Martin",
                    "lastName": "Nyffeler"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mirjam",
                    "lastName": "Knörnschild"
