            "volume": "66",
            "issue": "6",
            "pages": "655-666",
            "date": "2019",
            "series": "",
            "seriesTitle": "",
            "seriesText": "",
            "journalAbbreviation": "",
            "language": "en",
            "DOI": "10.1111/zph.12618",
            "ISSN": "1863-2378",
            "shortTitle": "Viral networks and detection of potential zoonotic viruses in bats and rodents",
            "url": "http://onlinelibrary.wiley.com/doi/abs/10.1111/zph.12618",
