            "key": "ZAV6S887",
            "version": 11593,
            "itemType": "journalArticle",
            "title": "Uncovering zoonoses awareness in an emerging disease 'hotspot'",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Sarah B.",
                    "lastName": "Paige"
                },
                {
                    "creatorType": "author",
                    "firstName": "Carly",
                    "lastName": "Malavé"
                },
                {
                    "creatorType": "author",
                    "firstName": "Edith",
                    "lastName": "Mbabazi"
