            "version": 56157,
            "itemType": "journalArticle",
            "title": "Consumer perceptions and reported wild and domestic meat and fish consumption behavior during the Ebola epidemic in Guinea, West Africa",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Lucie",
                    "lastName": "Duonamou"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alexandre",
                    "lastName": "Konate"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sylvie Djègo",
                    "lastName": "Djossou"
                },
                {
                    "creatorType": "author",
                    "firstName": "Guy Apollinaire",
                    "lastName": "Mensah"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jiliang",
                    "lastName": "Xu"
