            "version": 56178,
            "itemType": "journalArticle",
            "title": "Natural Hendra Virus Infection in Flying-Foxes - Tissue Tropism and Risk Factors",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Lauren K.",
                    "lastName": "Goldspink"
                },
                {
                    "creatorType": "author",
                    "firstName": "Daniel W.",
                    "lastName": "Edson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Miranda E.",
                    "lastName": "Vidgen"
                },
                {
                    "creatorType": "author",
                    "firstName": "John",
                    "lastName": "Bingham"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hume E.",
