            "version": 614,
            "itemType": "journalArticle",
            "title": "White-nose syndrome survivors do not exhibit frequent arousals associated with Pseudogymnoascus destructans infection",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Thomas Mikael",
                    "lastName": "Lilley"
                },
                {
                    "creatorType": "author",
                    "firstName": "Joseph Samuel",
                    "lastName": "Johnson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Lasse",
                    "lastName": "Ruokolainen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Elisabeth Jeannine",
