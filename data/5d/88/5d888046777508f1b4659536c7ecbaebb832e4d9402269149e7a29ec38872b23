            "version": 613,
            "itemType": "journalArticle",
            "title": "White-nose syndrome initiates a cascade of physiologic disturbances in the hibernating bat host",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Michelle L",
                    "lastName": "Verant"
                },
                {
                    "creatorType": "author",
                    "firstName": "Carol U",
                    "lastName": "Meteyer"
                },
                {
                    "creatorType": "author",
                    "firstName": "John R",
                    "lastName": "Speakman"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paul M",
