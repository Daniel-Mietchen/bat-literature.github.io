                {
                    "creatorType": "author",
                    "firstName": "Cali Ann",
                    "lastName": "Wilson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Spencer Mead",
                    "lastName": "Schell"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kenneth Alan",
