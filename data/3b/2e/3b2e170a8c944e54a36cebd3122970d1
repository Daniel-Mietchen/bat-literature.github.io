            "version": 35023,
            "itemType": "journalArticle",
            "title": "Abiotic and spatiotemporal factors affect activity of European bat species and have implications for detectability for acoustic surveys",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Samantha J.",
                    "lastName": "Perks"
                },
