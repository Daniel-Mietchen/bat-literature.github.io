            "version": 38532,
            "itemType": "journalArticle",
            "title": "Molecular Evolution of the Nuclear Factor (Erythroid-Derived 2)-Like 2 Gene Nrf2 in Old World Fruit Bats (Chiroptera: Pteropodidae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Qiuyuan",
                    "lastName": "Yin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Lei",
                    "lastName": "Zhu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Di",
                    "lastName": "Liu"
                },
                {
                    "creatorType": "author",
                    "firstName": "David M.",
                    "lastName": "Irwin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Shuyi",
                    "lastName": "Zhang"
