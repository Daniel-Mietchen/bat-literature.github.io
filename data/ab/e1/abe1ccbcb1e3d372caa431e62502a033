            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Michael A.",
                    "lastName": "Menzel"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jennifer M.",
                    "lastName": "Menzel"
                },
                {
                    "creatorType": "author",
                    "firstName": "John C.",
