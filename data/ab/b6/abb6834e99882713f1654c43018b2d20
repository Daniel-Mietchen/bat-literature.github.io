            "key": "WVNPL8V2",
            "version": 12764,
            "itemType": "journalArticle",
            "title": "Effects of roost specialization on extinction risk in bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Maria",
                    "lastName": "Sagot"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gloriana",
                    "lastName": "Chaverri"
