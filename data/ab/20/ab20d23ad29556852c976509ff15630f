                {
                    "creatorType": "author",
                    "firstName": "Carl",
                    "lastName": "Jones"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paul W.",
                    "lastName": "Howey"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jérémy S. P.",
                    "lastName": "Froidevaux"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paul A.",
                    "lastName": "Racey"
