            "key": "68EHNQR4",
            "version": 481,
            "itemType": "journalArticle",
            "title": "Risk factors associated with mortality from white-nose syndrome among hibernating bat colonies",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "a. P.",
                    "lastName": "Wilder"
                },
                {
                    "creatorType": "author",
                    "firstName": "W. F.",
                    "lastName": "Frick"
                },
                {
                    "creatorType": "author",
                    "firstName": "K. E.",
