            "version": 465,
            "itemType": "journalArticle",
            "title": "Receptor Recognition by the Novel Coronavirus from Wuhan: an Analysis Based on Decade-Long Structural Studies of SARS Coronavirus",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Yushun",
                    "lastName": "Wan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jian",
                    "lastName": "Shang"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rachel",
                    "lastName": "Graham"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ralph S.",
