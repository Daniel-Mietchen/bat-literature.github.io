            "version": 293,
            "itemType": "journalArticle",
            "title": "Highly sensitive quantitative PCR for the detection and differentiation of <i>Pseudogymnoascus destructans</i> and other <i>Pseudogymnoascus</i> species",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Megan M.",
                    "lastName": "Shuey"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kevin P.",
                    "lastName": "Drees"
                },
                {
                    "creatorType": "author",
                    "firstName": "Daniel L.",
                    "lastName": "Lindner"
                },
