            "key": "JAI7H6IF",
            "version": 11574,
            "itemType": "report",
            "title": "Townsend’s Big-eared Bat (Corynorhinus townsendii): a technical conservation assessment.",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jeffery C",
                    "lastName": "Gruver"
                },
                {
                    "creatorType": "author",
                    "firstName": "Douglas A",
                    "lastName": "Keinath"
                }
