            "key": "YFJKBTQS",
            "version": 11597,
            "itemType": "journalArticle",
            "title": "Lyssaviruses and rabies: current conundrums, concerns, contradictions and controversies",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Charles",
                    "lastName": "Rupprecht"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ivan",
                    "lastName": "Kuzmin"
                },
