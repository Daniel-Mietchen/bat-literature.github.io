            "version": 13683,
            "itemType": "journalArticle",
            "title": "Morphological Adaptations for Digging and Climate-Impacted Soil Properties Define Pocket Gopher (Thomomys spp.) Distributions",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ariel E.",
                    "lastName": "Marcy"
                },
                {
                    "creatorType": "author",
                    "firstName": "Scott",
                    "lastName": "Fendorf"
                },
                {
                    "creatorType": "author",
                    "firstName": "James L.",
                    "lastName": "Patton"
                },
                {
                    "creatorType": "author",
                    "firstName": "Elizabeth A.",
                    "lastName": "Hadly"
                },
                {
                    "creatorType": "editor",
                    "firstName": "Vincent",
                    "lastName": "Laudet"
                }
