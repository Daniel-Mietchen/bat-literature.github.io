            "key": "Y49G4LTA",
            "version": 87,
            "itemType": "journalArticle",
            "title": "Beyond the Cut Hunter: A Historical Epidemiology of HIV Beginnings in Central Africa",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Stephanie",
                    "lastName": "Rupp"
                },
                {
                    "creatorType": "author",
                    "firstName": "Philippe",
                    "lastName": "Ambata"
                },
                {
                    "creatorType": "author",
                    "firstName": "Victor",
                    "lastName": "Narat"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tamara",
                    "lastName": "Giles-Vernick"
                }
