            "key": "SP4KSIET",
            "version": 805,
            "itemType": "journalArticle",
            "title": "Can Plant Viruses Cross the Kingdom Border and Be Pathogenic to Humans?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Fanny",
                    "lastName": "Balique"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hervé",
                    "lastName": "Lecoq"
                },
                {
                    "creatorType": "author",
                    "firstName": "Didier",
