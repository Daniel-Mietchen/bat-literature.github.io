            "version": 38495,
            "itemType": "journalArticle",
            "title": "Biodiversity and metacommunity structure of animals along altitudinal gradients in tropical montane forests",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Michael R.",
                    "lastName": "Willig"
                },
