            "version": 18240,
            "itemType": "journalArticle",
            "title": "Effects of land use on bat diversity in a complex plantation–forest landscape in northeastern Brazil",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Katrin",
                    "lastName": "Heer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Maria",
                    "lastName": "Helbig-Bonitz"
                },
                {
                    "creatorType": "author",
                    "firstName": "Renato G.",
