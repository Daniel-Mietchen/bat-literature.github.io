                {
                    "creatorType": "author",
                    "firstName": "M. N.",
                    "lastName": "Wang"
                },
                {
                    "creatorType": "author",
                    "firstName": "P.",
                    "lastName": "Daszak"
                },
                {
                    "creatorType": "author",
                    "firstName": "L. F.",
                    "lastName": "Wang"
                },
