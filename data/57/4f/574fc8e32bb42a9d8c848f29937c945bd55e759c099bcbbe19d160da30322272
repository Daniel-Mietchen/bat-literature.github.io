            "key": "SPHQWYTK",
            "version": 582,
            "itemType": "journalArticle",
            "title": "Torpor, arousal and activity of hibernating Greater Horseshoe Bats (Rhinolophus ferrumequinum)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "K. J.",
                    "lastName": "Park"
                },
                {
                    "creatorType": "author",
                    "firstName": "G.",
                    "lastName": "Jones"
                },
