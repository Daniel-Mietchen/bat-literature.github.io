            "key": "ICFS927C",
            "version": 11562,
            "itemType": "journalArticle",
            "title": "Crimean-Congo hemorrhagic fever: History, epidemiology, pathogenesis, clinical syndrome and genetic diversity",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Dennis A.",
                    "lastName": "Bente"
                },
                {
                    "creatorType": "author",
                    "firstName": "Naomi L.",
                    "lastName": "Forrester"
                },
                {
                    "creatorType": "author",
                    "firstName": "Douglas M.",
                    "lastName": "Watts"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alexander J.",
