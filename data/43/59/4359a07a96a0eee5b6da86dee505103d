            "version": 13586,
            "itemType": "journalArticle",
            "title": "No energetic cost of anthropogenic disturbance in a songbird",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Isabelle-Anne",
                    "lastName": "Bisson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Luke K",
                    "lastName": "Butler"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tim J",
                    "lastName": "Hayden"
                },
                {
                    "creatorType": "author",
                    "firstName": "L. Michael",
                    "lastName": "Romero"
                },
