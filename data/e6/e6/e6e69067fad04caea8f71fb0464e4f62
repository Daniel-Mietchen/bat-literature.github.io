            "version": 58475,
            "itemType": "journalArticle",
            "title": "Inside or out? Cave size and landscape effects on cave-roosting bat assemblages in Brazilian Caatinga caves",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Juan Carlos",
                    "lastName": "Vargas-Mena"
                },
                {
                    "creatorType": "author",
                    "firstName": "Eugenia",
                    "lastName": "Cordero-Schmidt"
                },
                {
                    "creatorType": "author",
                    "firstName": "Bernal",
                    "lastName": "Rodriguez-Herrera"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rodrigo A",
                    "lastName": "Medellín"
                },
                {
                    "creatorType": "author",
                    "firstName": "Diego De Medeiros",
                    "lastName": "Bento"
                },
                {
                    "creatorType": "author",
                    "firstName": "Eduardo M",
                    "lastName": "Venticinque"
