            "version": 58613,
            "itemType": "journalArticle",
            "title": "COMPOSITION AND STRUCTURE OF BAT ASSEMBLAGES IN PARAGUAY: A SUBTROPICAL–TEMPERATE INTERFACE",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Michael R",
                    "lastName": "Willig"
                },
                {
                    "creatorType": "author",
                    "firstName": "Steven J",
                    "lastName": "Presley"
                },
