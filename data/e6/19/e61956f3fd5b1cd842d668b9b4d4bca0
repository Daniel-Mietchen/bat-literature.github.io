            "version": 32694,
            "itemType": "journalArticle",
            "title": "Proteomic analysis of Pteropus alecto kidney cells in response to the viral mimic, Poly I:C",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Lawrence",
                    "lastName": "Mok"
                },
                {
                    "creatorType": "author",
                    "firstName": "James W.",
                    "lastName": "Wynne"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kris",
                    "lastName": "Ford"
                },
                {
                    "creatorType": "author",
                    "firstName": "Brian",
                    "lastName": "Shiell"
