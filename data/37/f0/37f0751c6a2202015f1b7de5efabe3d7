            "version": 42448,
            "itemType": "journalArticle",
            "title": "Evolution of scapula shape in several families of bats (Chiroptera, Mammalia)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Pablo J.",
                    "lastName": "Gaudioso"
                },
                {
                    "creatorType": "author",
                    "firstName": "Juan J.",
                    "lastName": "Martínez"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rubén M.",
