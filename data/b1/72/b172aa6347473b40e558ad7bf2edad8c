        "data": {
            "key": "VCQRK665",
            "version": 15454,
            "itemType": "journalArticle",
            "title": "Serological assays for emerging coronaviruses: Challenges and pitfalls",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Benjamin",
                    "lastName": "Meyer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christian",
                    "lastName": "Drosten"
                },
