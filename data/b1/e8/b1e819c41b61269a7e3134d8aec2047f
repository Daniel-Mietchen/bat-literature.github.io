            "version": 13774,
            "itemType": "journalArticle",
            "title": "SCALE-DEPENDENT EFFECTS OF LANDSCAPE CONTEXT ON THREE POLLINATOR GUILDS",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ingolf",
                    "lastName": "Steffan-Dewenter"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ute Mu",
                    "lastName": "Nzenberg"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christof Bu",
