            "key": "VLKV6LSZ",
            "version": 13766,
            "itemType": "journalArticle",
            "title": "Rodent trapping studies as an overlooked information source for understanding endemic and novel zoonotic spillover",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "David",
                    "lastName": "Simons"
                },
                {
                    "creatorType": "author",
                    "firstName": "Lauren A.",
                    "lastName": "Attfield"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kate E.",
                    "lastName": "Jones"
                },
                {
                    "creatorType": "author",
                    "firstName": "Deborah",
                    "lastName": "Watson-Jones"
                },
